    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The switch port that the real server is connected to." 
    ::= { slbRealServerInfoEntry 4 }

slbRealServerInfoHealthLayer  OBJECT-TYPE
    SYNTAX  INTEGER { 
        other(1), 
        layer3(2),
        layer4(3)  
        } 
