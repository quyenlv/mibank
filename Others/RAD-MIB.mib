pktSwchAsyncConnectionStatus OBJECT-TYPE
    SYNTAX INTEGER {
	  engaged(1),
	  free   (2)
	  }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The connection status of the channel is either FREE or ENG (engaged)."
    ::= { pktSwchAsyncStatEntry 3}

pktSwchAsyncNumLogChannels OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The number of active logical channels used by the asynchronous channel."
    ::= { pktSwchAsyncStatEntry 4 }

pktSwchAsyncNumActSessions OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The number of active sessions opened by this asynchronous channel."
    ::= { pktSwchAsyncStatEntry 5 }

pktSwchAsyncAutocallId OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"Indicates whether the channel has been configured to use the autocall function and
		 what is the call  ID.  NONE means that the channel is not configured for autocall."
    ::= { pktSwchAsyncStatEntry 6}

pktSwchAsyncRTSStatus OBJECT-TYPE
    SYNTAX INTEGER {
	  unused(1),
	  off   (2),
	  on    (3)
	  }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"Shows the status of the RTS line of the asynchronous channel.
	 OFF indicates that the RTS line is not active, ON indicates that
	 it is active, and UNUSED indicates that the channel PAD is
	 programmed to disregard the state of the RTS line."
    ::= { pktSwchAsyncStatEntry 7 }

pktSwchAsyncBuffToNet OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The number of data buffers currently used by the transmit direction of the channel."
    ::= { pktSwchAsyncStatEntry 8 }

pktSwchAsyncBuffToUser OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The number of data buffers currently used by the receive direction of the channel."
    ::= { pktSwchAsyncStatEntry 9}

pktSwchAsyncTxXoff OBJECT-TYPE
    SYNTAX INTEGER {
      no (1),
      yes(2)
      }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The current state of software flow control in the transmit direction.
	 NO indicates that traffic flow is enabled.  YES indicates that traffic
	 flow from the user's DTE to the device channel has been interrupted
	 by sending the X-OFF character to the DTE."
    ::= { pktSwchAsyncStatEntry 10 }


pktSwchAsyncRxXoff OBJECT-TYPE
    SYNTAX  INTEGER {
      no (1),
      yes(2)
      }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The current state of software flow control in the receive direction.
	 NO indicates that traffic flow is enabled.  YES indicates that traffic
	 flow to the user's DTE from the device channel has been interrupted
	 by means of the X-OFF character."
    ::= { pktSwchAsyncStatEntry 11 }

pktSwchAsyncBuffOutstand OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"Indicates the number of buffers which were sent to the async user
	 but weren't yet freed."
    ::= { pktSwchAsyncStatEntry 12 }

pktSwchAsyncChannelMode OBJECT-TYPE
    SYNTAX INTEGER {
	  none        (1),
	  activeLink  (2),
	  x28command  (3),
	  dataTransfer(4)
	  }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The channel mode is either X28 COMMAND or DATA TRANSFER."
    ::= { pktSwchAsyncStatEntry 13 }


-- The pktSwchAsyncRunProTable contains status information and statistics for the Async channels

