Counter32,
pktSwchEthernetLinkIpFramesIn
Counter32,
pktSwchEthernetLinkIpFramesOut
Counter32,
pktSwchEthernetLinkLlc2FramesIn
Counter32,
pktSwchEthernetLinkLlc2FramesOut
Counter32,
pktSwchEthernetLinkBridgFramesIn
Counter32,
pktSwchEthernetLinkBridgFramesOut
Counter32,
pktSwchEthernetLinkDiscardedByFilter
Counter32
     }

pktSwchEthernetLinkOperationMode  OBJECT-TYPE
    SYNTAX INTEGER{
        bridging         (3),
        ip               (4),
        ip-and-bridging  (7),
        llc              (8),
        llc-and-bridging (11),
        ip-and-llc       (12),
        all              (15),
        none             (255) -- disabled
	       }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "This parameter shows the frame handling mode on this link.

         When IP mode is defined, the IP frames received on this link
         are handled and routed by the IP module in the PS.
         If IP mode is disabled, the IP frames will not be handled by the
         IP module and will be handled as bridged data.

         When Bridging mode is enabled, the PS will perform bridging from
         this link according to the bridging configuration. If disabled,
         bridge data will be ignored.

         When LLC is enabled, frames received on this link are handled by
         the LLC module in the PS. If disabled, the LLC frames will be
         treated as bridged data.

         Any combination of bridging, ip and LLC is allowed. If both IP
         and LLC are enabled, LLC frames will be handled by the LLC module,
         IP frames will be handled by the IP module, and all the other frames
         will treated as bridged data. If the bridge mode is disabled, the
         other frames will be discarded."

    ::= { pktSwchEthernetLinkEntry 1 }


