pktSwchMng  OBJECT IDENTIFIER ::= { pktSwchSystem 4 }

pktSwchMngX25TrapIpTable  OBJECT-TYPE
	SYNTAX  SEQUENCE OF PktSwchMngX25Entry
	MAX-ACCESS  not-accessible
	STATUS current
	DESCRIPTION
	"This table indicates  the destination address of the traps.
	  Internally it will be limited to ten addresses. "
	::= { pktSwchMng 1 }

pktSwchMngX25Entry OBJECT-TYPE
	SYNTAX PktSwchMngX25Entry
	MAX-ACCESS  not-accessible
	STATUS current
    DESCRIPTION
	"An entry in table pktSwchMngX25TrapIpTable."
	INDEX {  pktSwchMngID }
	::= {pktSwchMngX25TrapIpTable  1 }


PktSwchMngX25Entry  ::=
	    SEQUENCE {
	pktSwchMngID
	  Integer32,
	pktSwchMngIP
	  IpAddress,
	pktSwchMngPhyAddr
	  DisplayString,
	pktSwchTrapEnable
	  Integer32,
	pktSwchEncapsulationMode
	  INTEGER
	}

pktSwchMngID OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-only
	STATUS current
	DESCRIPTION
	" This value indicates an index of a specific IP address"
	::= { pktSwchMngX25Entry 1 }

pktSwchMngIP OBJECT-TYPE
	SYNTAX  IpAddress
	MAX-ACCESS  read-write
        STATUS  current
	DESCRIPTION
	"This value indicates  the  destination address of the traps. IP 0.0.0.0 removes the entry. "
	::= { pktSwchMngX25Entry 2 }

pktSwchMngPhyAddr OBJECT-TYPE
	SYNTAX  DisplayString
	MAX-ACCESS  read-write
	STATUS  deprecated
	DESCRIPTION
	"This value indicates  the  physical destination  address of the traps."
	::= { pktSwchMngX25Entry 3 }

pktSwchTrapEnable OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-write
        STATUS  current
	DESCRIPTION
	"This value indicates which traps should be sent.
	 The value is a sum of values (powers of two), where each value in
	 the sum represents	a trap that should be reported to the Management
	 Station. The values that may be included in the sum and their meanings are:
		0   No traps would be reported.
		1   Link Down
		2   Link Up
		4   rtsChange
		8   resetX25
		16  restartX25
		32  frDLCIStatusChange
		64  coldStart
		128 linkProtocolChanged
        256 enrollmentPS
		512 ledStatusChange         "
	::= { pktSwchMngX25Entry 4 }

pktSwchEncapsulationMode OBJECT-TYPE
    SYNTAX  INTEGER	{
	 ipPid(1),
	 snapPid(2)
	 }
	MAX-ACCESS  read-write
        STATUS  deprecated
	DESCRIPTION
	"Format type for IP over X25"
	::= { pktSwchMngX25Entry 5 }


