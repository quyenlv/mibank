       cugIa(3),   --  CUG/IA with preferential
       cugOaIa(4),   --  CUG/OA/IA with preferential
       noPref(5),    -- CUG/OA/IA without preferential (transparent)
       noCug(255)  -- no CUG
     }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"The CUG subscription for the link."
    ::= { pktSwchPlpSEntry 4 }

