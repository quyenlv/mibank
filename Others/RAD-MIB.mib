	 INTEGER,
	pktSwchAsyncRunProLineFeedPad
	 Integer32,
	pktSwchAsyncRunProEditing
	 INTEGER,
	pktSwchAsyncRunProCharDel
	 Integer32,
	pktSwchAsyncRunProLineDel
	 Integer32,
