pktSwchIsdnRoutTable    OBJECT-TYPE
    SYNTAX  SEQUENCE  OF PktSwchIsdnRoutEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
	"The Isdn Routing Table entries in the Packet Switch."
     ::=  { pktSwchIsdn 2 }

pktSwchIsdnRoutEntry OBJECT-TYPE
    SYNTAX  PktSwchIsdnRoutEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "Isdn Routing Entry defines the contents of each entry in the table.
         The outgoing addresses are in a different table -
         pktSwchIsdnRoutAddrTable. The row status in this
         table will be used for both tables."
         INDEX   { pktSwchIsdnRoutId }
         ::=  { pktSwchIsdnRoutTable 1 }

PktSwchIsdnRoutEntry ::=  SEQUENCE {
    pktSwchIsdnRoutId
    Integer32,
    pktSwchIsdnRoutDataBandWidth
     INTEGER,
    pktSwchIsdnRoutOptions
     INTEGER,
    pktSwchIsdnRoutStatus
     INTEGER
    }

pktSwchIsdnRoutId OBJECT-TYPE
    SYNTAX Integer32 (1..200)
        MAX-ACCESS  read-only
	STATUS  current
	DESCRIPTION
        "Number of this routing entry."
        ::= { pktSwchIsdnRoutEntry 1 }

pktSwchIsdnRoutDataBandWidth OBJECT-TYPE
    SYNTAX  INTEGER {
        bw56Kbps(2),
        bw64Kbps(3)
	}
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
    "Isdn Bandwidth which will be used for the data routed via this
     entry. The ISDN interface will be configured with the speed
     which is selected.
     Possible values:
     2 - 56 Kbps.
     3 - 64 Kbps.
     Default value: 3 "
    ::= { pktSwchIsdnRoutEntry 2 }

pktSwchIsdnRoutOptions OBJECT-TYPE
    SYNTAX  INTEGER {
        noOptions(1),
        returnToMainLink(2)
	}
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
    "Special Options for this routing entry.
     Possible values:
     1 - No special options.
     2 - Return to main link. This option is used when the current link
         is defined as a backup link and the main link becomes active.
     Default value: 1 "
    ::= { pktSwchIsdnRoutEntry 3 }

pktSwchIsdnRoutStatus OBJECT-TYPE
    SYNTAX  INTEGER {
       valid(1),
       createRequest(2),
       underCreation(3),
       invalid(4)
     }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
    "The status of this routing table entry + address table
     (pktSwchIsdnOutAddrTable).
     For creating a new entry select createRequest. A new route entry
     will be created together with an out address entry. There may be
     more than 1 addresses. For example, if there is 1 outgoing address
     and 2 alternatives, the new address entry will have 3 indices.
     For deleting an entry select invalid. The relevant address entry
     should be deleted too.
     For saving an entry select valid. Save should include the addresses
     of the current entry in the pktSwchIsdnOutAddrTable"
     ::= { pktSwchIsdnRoutEntry 4 }


pktSwchIsdnOutAddrTable    OBJECT-TYPE
    SYNTAX  SEQUENCE  OF PktSwchIsdnOutAddrEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
    "Outgoing ISDN addresses used by the ISDN routing table.
     The address table contains outgoing
     addresses and alternative addresses in case the connection
     to the first addresses does not succeed."
