-- Module specific for Token Ring


mdlIBTable OBJECT-TYPE
	       SYNTAX SEQUENCE OF MdlIBEntry
	       MAX-ACCESS not-accessible
	       STATUS current
	       DESCRIPTION
		   "Additional parameters that can be monitored for a segment
		    with an inband module. Module specific for Token Ring"
	       ::= { moduleRR 2 }

mdlIBEntry OBJECT-TYPE
	       SYNTAX MdlIBEntry
	       MAX-ACCESS not-accessible
	       STATUS current
	       DESCRIPTION
		   "An entry in table mdlIBTable."
	       INDEX { mdlIBid}
	       ::= { mdlIBTable 1}

MdlIBEntry ::= 
	    SEQUENCE {
	       mdlIBid
		  Integer32,
	       mdlIBsegID
		  Integer32,
	       mdlIBCorrelationEnable
		  INTEGER,
	       mdlIBAutorecEnable
		  INTEGER,
	       mdlIBAutorecResult
		 INTEGER,
	       mdlIBcommand
		 INTEGER,
	       mdlIBstate
		 INTEGER,
	       mdlIBagnt
    	 INTEGER,
	       mdlIBIpAddress
		 IpAddress,
	       mdlIBLocalMacAddr
		 MacAddress,
	       mdlIBbrnInMacAddr
		 MacAddress,
	       mdlIBLocalAddrStatus
		 INTEGER,
	       mdlIBTrapMaskEnable
	 INTEGER
	 }

mdlIBid OBJECT-TYPE
	   SYNTAX  Integer32  (1..20)
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	   "Unique number to identify the CML\IB module. It indicates the slot
	    number the module is inserted in"
	   ::= {  mdlIBEntry 1 }

mdlIBsegID OBJECT-TYPE
	   SYNTAX  Integer32  (1..20)
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	   "Unique number to identify the segment where the CML\IB module
	    is inserted. It is one of the segTRID values."
	   ::= {  mdlIBEntry 2 }

mdlIBCorrelationEnable OBJECT-TYPE
	   SYNTAX  INTEGER {
		off(2),
		on(3)
		     }
	   MAX-ACCESS  read-write
	   STATUS  current
	   DESCRIPTION
	   "When set to ON(3), the in-band agent should perform 
	    correlation each time a station enters/leaves the segment."
	   ::= { mdlIBEntry 3 }

mdlIBAutorecEnable OBJECT-TYPE
	   SYNTAX  INTEGER {
		off(2),
		on(3)
		     }
	   MAX-ACCESS  read-write
	   STATUS  current
	   DESCRIPTION
	   "When set to ON(3), the in-band agent should perform 
	    autorecovery each time a beacon status is detected."
	   ::= { mdlIBEntry 4 }

mdlIBAutorecResult OBJECT-TYPE
	   SYNTAX  INTEGER {
		notStarted(1),
		started(2),
		segmentOk(3),
		segmentFixed(4),
		segmentClosed(5),
		failed(6),
		hubProblem(7)
		     }
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	    "When autorecovery is completed the result will be reported (3)..(7).
	     While in autorecovery process, started(2) will be reported.
	     Before autorecovery process, notStarted(1) will be reported."  
	   ::= { mdlIBEntry 5 }

mdlIBcommand   OBJECT-TYPE
	 SYNTAX  INTEGER {
		       noOp(1),
		       open(2),
		       reset(3),
		       close(4)
		 }
	 MAX-ACCESS  read-write
	 STATUS  current
	 DESCRIPTION
		 "When this object is set to the value of
		  open(2), the station should go into the
		  open state.  The progress and success of
		  this command is given by the values of the
		  object mdlIBstate.
		      When this object is set to the value
		  of reset(3), then the station should do
		  a reset.  On a reset, all MIB counters
		  should retain their values, if possible.
		  Other side effects depend on the hardware chip set.
		      When this object is set to the value
		  of close(4), the station should go into
		  the closed state by removing itself
		  from the ring.
		      Setting this object to a value of
		  no-op(1) has no effect.
		      For read operation, this object has always a
		  value of no-op(1). It is not permitted to apply this 
	      command on an agent module."
	 ::= { mdlIBEntry 6 }

mdlIBstate OBJECT-TYPE
		     SYNTAX  INTEGER
		{ 
		unknown (1),
		opened(2),
		closed(3),
		opening(4),
		closing(5),
		badParam(6),
		lobeFailed(7),
		signalLoss(8),
		insertionTimeout(9),
		ringFailed(10),
		beaconning(11),
		duplicateMac(12),
		requestFailed(13),
		removeReceived(14),
		budFailure(15),
		initFailure(16)
		}                       
		     MAX-ACCESS  read-only
		     STATUS  current
		     DESCRIPTION
			     "The current adapter status, which can
			     be used to diagnose fluctuating problems."
	::= { mdlIBEntry 7 }

mdlIBagnt OBJECT-TYPE
	   SYNTAX  INTEGER  {
		off(2),
		on (3),
		standBy (4)
    	}
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	" Indicates whether the specific inband module serves as:
	  - Active hub agent - (on)
	  - Ring Monitor (for TR module only) - (off)
	  - Standby agent - (standBy)"
	::= { mdlIBEntry 8 }

mdlIBIpAddress OBJECT-TYPE
      SYNTAX  IpAddress
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Indicates IP address of the module if the module serves as  
       an agent of itself."
      ::= {  mdlIBEntry 9}

mdlIBLocalMacAddr OBJECT-TYPE
	   SYNTAX  MacAddress
	   MAX-ACCESS  read-write
	   STATUS  current
	   DESCRIPTION
	   "The value of this object is the specific MAC address 
	   of this interface which uniquely identifies this interface 
	   on a ring.It is the address set by the management  and
           upon request it will replace the burn in address.
           Non-canonical format."
	   ::= { mdlIBEntry 10}

mdlIBLocalAddrStatus OBJECT-TYPE
	   SYNTAX  INTEGER {
		   unknown(1),
		   off(2),
		   on(3)
		}
	   MAX-ACCESS  read-write
	   STATUS  current
	   DESCRIPTION
	   "The value of this object specifies whether the specific 
	    MAC address  of this interface was set by the local 
	    management or it is the burn-in address in use."
	   ::= { mdlIBEntry 11 }

mdlIBbrnInMacAddr OBJECT-TYPE 
      SYNTAX   MacAddress
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	"The value of this object is the specific Burn-in MAC address 
	   of this interface which uniquely identifies this interface 
	   on a ring. Non-canonical format."
	   ::= { mdlIBEntry 12 }

mdlIBTrapMaskEnable OBJECT-TYPE
	   SYNTAX  INTEGER {
	    off(2),
	   on(3)
      }
      MAX-ACCESS  read-write    
      STATUS  current
      DESCRIPTION
      "When this attribute is set to OFF by the management console, the 
       agent starts sending the unmasked traps related to this module. 
       Otherwise, no traps are sent by this module."
	   ::= { mdlIBEntry 13 }

-- Special modules such as bridges/converters  that are within the hub
--

mdlBRGTable OBJECT-TYPE
	       SYNTAX SEQUENCE OF MdlBRGEntry
	       MAX-ACCESS not-accessible
	       STATUS current
	       DESCRIPTION
		   "Special modules such as bridges/converters  that are within the hub"
	       ::= { moduleRR 3 }

mdlBRGEntry OBJECT-TYPE
	       SYNTAX MdlBRGEntry
	       MAX-ACCESS not-accessible
	       STATUS current
	       DESCRIPTION
		   "An entry in table mdlBRGTable."
	       INDEX { mdlBRGid}
	       ::= { mdlBRGTable 1}

MdlBRGEntry ::= 
	    SEQUENCE {
	  mdlBRGid
	      Integer32,
	   mdlBRGReady
	      INTEGER,
	   mdlBRGStatus
	       DisplayString,
	    mdlBRGLocation
	       INTEGER,
	    mdlBRGIpAddress
	       IpAddress,
	    mdlBRGMacAddr
	       MacAddress,
		mdlBRGAdapterState
		   INTEGER,
		mdlBRGBypassSw
		   INTEGER
	 }

mdlBRGid OBJECT-TYPE
	   SYNTAX  Integer32  (1..20)
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	   "Unique number to identify the module. It indicates the slot
	    number the module is inserted in."
	 ::= { mdlBRGEntry 1}

 mdlBRGReady  OBJECT-TYPE   
      SYNTAX  INTEGER {  
	   unknown(1),
	   off(2), 
	   on(3),
	    blink(4)
      }
      MAX-ACCESS  read-only    
      STATUS  current
      DESCRIPTION
       "This entry will have the value ON when packets can be transferred in the TRE module.
	 The value UNKNOWN will be used for all other modules where this characteristic is 
	not applicable"
	 ::= { mdlBRGEntry 2}

mdlBRGStatus OBJECT-TYPE   
      SYNTAX  DisplayString      
      MAX-ACCESS  read-only    
      STATUS  current
      DESCRIPTION
      "This entry specifies the status of operation of the STC or FTC modules. For other modules
	this value will read NOAP"
 ::= { mdlBRGEntry 3}

 mdlBRGLocation  OBJECT-TYPE   
      SYNTAX  INTEGER {  
	   unknown(1),
	   main(2), 
	   remote(3),
	   secondary(4)
      }
      MAX-ACCESS  read-only    
      STATUS  current
      DESCRIPTION
       "This entry indicates the configuration mode:
        - for the TRE bridge (main/remote) 
        - for MLB-T, MLBT-F (main/secondary) "
 ::= { mdlBRGEntry 4}

mdlBRGIpAddress OBJECT-TYPE
      SYNTAX  IpAddress
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Indicates IP address of the module if the module serves as  an agent of itself. For 
       all other modules the IP value will be 0.0.0.0"
      ::= {  mdlBRGEntry 5}


mdlBRGMacAddr OBJECT-TYPE 
      SYNTAX   MacAddress
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	"The value of this object is the specific MAC address 
	   of this interface which uniquely identifies this interface 
	   on a ring."
	   ::= { mdlBRGEntry 6}

mdlBRGAdapterState OBJECT-TYPE
		     SYNTAX  INTEGER
		{ 
		unknown (1),
		opened(2),
		closed(3),
		opening(4),					  
		closing(5),
		badParam(6),
		lobeFailed(7),
		signalLoss(8),
		insertionTimeout(9),
		ringFailed(10),
		beaconning(11),
		duplicateMac(12),
		requestFailed(13),
		removeReceived(14),
		budFailure(15),
		initFailure(16)
		}                       
		     MAX-ACCESS  read-only
		     STATUS  current
		     DESCRIPTION
			     "The current adapter status, which can
			      be used to diagnose fluctuating problems.
			      This object is applicable only for TR bridges
			      with adapter.
			      The value 'unknown' will be used for other 
			      module types."
	::= { mdlBRGEntry 7 }

mdlBRGBypassSw  OBJECT-TYPE   
      SYNTAX  INTEGER {  
	   unknown(1),
	   off(2), 
	   on(3)
      }
      MAX-ACCESS  read-only    
      STATUS  current
      DESCRIPTION
       "This entry is applicable only for FTB-D/FO module.
        It describes the status of the Bypass Switch. 
        The value UNKNOWN will be used for all other modules where 
        this characteristic is not applicable"
	 ::= { mdlBRGEntry 8}



-- Special "WAN" modules that are within the RadRing hub
--

mdlMMTable OBJECT-TYPE
	       SYNTAX SEQUENCE OF MdlMMEntry
	       MAX-ACCESS not-accessible
	       STATUS current
	       DESCRIPTION
		   "Special 'WAN' modules that are within the RadRing hub"
	       ::= { moduleRR 4 }

mdlMMEntry OBJECT-TYPE
	       SYNTAX MdlMMEntry
	       MAX-ACCESS not-accessible
	       STATUS current
	       DESCRIPTION
		   "An entry in table mdlMMTable."
	       INDEX { mdlMMid}
	       ::= { mdlMMTable 1}

MdlMMEntry ::= 
	    SEQUENCE {
	  mdlMMid
	      Integer32,
	   mdlMMBertCmd
	      INTEGER
	 }

mdlMMid OBJECT-TYPE
	   SYNTAX  Integer32  (1..20)
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	   "Unique number to identify the module. It indicates the slot
	    number the module is inserted in."
	 ::= { mdlMMEntry 1}

mdlMMBertCmd OBJECT-TYPE
	   SYNTAX  INTEGER {
	     unknown(1),
		 off(2),
		 on(3)
		     }
	   MAX-ACCESS  read-write
	   STATUS  current
	   DESCRIPTION
	   "Setting this Object to ON, will start BERT test on all the channels that are currently in               
	     Remote Loop. If the value of mdlRRMngState is disabled, this parameter is read-only
		     and  reflects the module jumper setting. "
	   ::= { mdlMMEntry 2}

-- "Ethernet" modules that are within the hub
--

mdlETTable OBJECT-TYPE
	       SYNTAX SEQUENCE OF MdlETEntry
	       MAX-ACCESS not-accessible
	       STATUS current
		   DESCRIPTION
		   "'Ethernet' modules that are within the hub"
	       ::= { moduleRR 5 }

mdlETEntry OBJECT-TYPE
	       SYNTAX MdlETEntry
	       MAX-ACCESS not-accessible
	       STATUS current
		   DESCRIPTION
		   "An entry in table mdlETTable."
	       INDEX { mdlMMid }
	       ::= { mdlETTable 1}

MdlETEntry ::= 
	    SEQUENCE {
       mdlETid
	      Integer32,
	   mdlETSAmode
	      INTEGER
	 }

mdlETid OBJECT-TYPE
	   SYNTAX  Integer32  (1..20)
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	   "Unique number to identify the module. It indicates the slot
	    number the module is inserted in. It is the same
		as rptrGroupIndex in the Repeater MIB"
	 ::= { mdlETEntry 1}

mdlETSAmode       OBJECT-TYPE
      SYNTAX   INTEGER {
	       unknown(1),
	       off(2),
	       on(3)
}
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Setting this Object to ON, will change the mode of operation 
       of the Ethernet card to a stand alone operation mode. It will 
       disconnect the card from the repeater bus. The rptrGroupOperStatus 
	   parameter in the Repeater MIB is changed to other(1). 
	   The parameter is read-only when the value of  mdlRRMngState 
	   is disabled"
	 ::= { mdlETEntry 2 }

--
--RADRing specific port information
--

prtRRTable OBJECT-TYPE
      SYNTAX SEQUENCE OF PrtRREntry
      MAX-ACCESS not-accessible
      STATUS current
      DESCRIPTION
	  "RADRing specific port information"
      ::= { prtRR 1 }

prtRREntry OBJECT-TYPE
      SYNTAX    PrtRREntry
      MAX-ACCESS    not-accessible
      STATUS    current
      DESCRIPTION
	  "An entry in table prtRRTable."
      INDEX     { prtRRmdlID, prtRRID }
      ::= { prtRRTable 1 }
 
PrtRREntry ::=
      SEQUENCE {
	  prtRRmdlID
	     Integer32,
	  prtRRID
	     Integer32,
	  prtRRSecurityState
	      INTEGER,
	  prtRRSecurityEnable
	      INTEGER,
	  prtRRSecurityMask  
	     MacAddress,
	  prtRRSecurityPattern
	     MacAddress,
	  prtRRJitterAttenEnable 
	     INTEGER,
	  prtRRConfigMsg
	      OCTET STRING,
	  prtRRFlipAutoDisable
	     INTEGER,
	  prtRRSecurityMask2
	     MacAddress,
	  prtRRSecurityPattern2
	     MacAddress,
	  prtRRMode
	      INTEGER,
	   prtRRAttachment
	      INTEGER
		 }

prtRRmdlID OBJECT-TYPE  
      SYNTAX  Integer32   
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "A unique identifier of the module,(slot no.),this port is attached to."
      ::= { prtRREntry 1 }

prtRRID OBJECT-TYPE  
      SYNTAX  Integer32   
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "A unique identifier of the port within its module."
      ::= { prtRREntry 2 }

prtRRSecurityState OBJECT-TYPE
	   SYNTAX  INTEGER {
		 notactive(1),
		 active(2)
		     }
	   MAX-ACCESS  read-only
	   STATUS  current
	   DESCRIPTION
	   "This entry indicates whether a security check can be performed on 
	    the specific port. This value is directly related to the
	    presence of an inband module in the segment where the port
	    belongs to and to activation of the correlation process."
	   ::= {  prtRREntry 3 }

prtRRSecurityEnable OBJECT-TYPE
      SYNTAX  INTEGER 
	{
	    off(2),
	    on(3)
	}
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "This value indicates whether a security check should be performed.
       Setting this value to ON(3) will activate the security mechanism
       if and only if, the value of  prtRRSecurityState is ACTIVE."
      ::= { prtRREntry 4 }


prtRRSecurityMask OBJECT-TYPE
      SYNTAX  MacAddress
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Mask of the allowed MAC address that can be connected through 
      this port. The security of the individual port overwrites  the
       security of the segment where the port belongs to"
      ::= { prtRREntry 5 }

prtRRSecurityPattern   OBJECT-TYPE
      SYNTAX  MacAddress
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "A six-octet(48 bit) pattern, where the masked bits of prtRRSecurityMask
       should match the MAC address."
      ::= { prtRREntry 6}

prtRRJitterAttenEnable OBJECT-TYPE
      SYNTAX  INTEGER {
	   unknown(1),
	   off(2),    
	   on(3)    
      }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "The value of this attribute determines activation of the
      jitters Attenuation mechanism of  repeater's port ."
      ::= { prtRREntry 7 }

 prtRRConfigMsg OBJECT-TYPE   
      SYNTAX  OCTET STRING
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Full configuration message  coded in internal RAD application format.
      This entry describes  port' configuration. "
      ::= { prtRREntry  8 }

prtRRFlipAutoDisable OBJECT-TYPE   
      SYNTAX   INTEGER {
		noApplicable(1),
		off(2),
		on(3)
	}
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Attribute describing the control on the flipping mechanism of a lobe . 
	Value ON indicates  the ability of the lobe to  auto disable itself when flipping occur. 
	Value   OFF indicates  that  the auto disable mechanism should not be activated and the 
	lobe should stay in a flipping state. 
       This attribute applies on selected lobes (tl-2/f, or tl-2/fsat)"
      ::= { prtRREntry  9 }

prtRRSecurityMask2 OBJECT-TYPE
      SYNTAX  MacAddress
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Mask of the allowed MAC address that can be connected through 
      this port. The security of the individual port overwrites  the
       security of the segment where the port belongs to"
      ::= { prtRREntry 10 }

prtRRSecurityPattern2   OBJECT-TYPE
      SYNTAX  MacAddress
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "A six-octet(48 bit) pattern: where the masked bits of prtRRSecurityMask2
       should match the MAC address."
      ::= { prtRREntry 11 }

prtRRMode  OBJECT-TYPE
      SYNTAX  INTEGER
{
		noApplicable (1),
		smart-port   (2),
		port         (3),
		interRptrPort(4),
		multiConnPort(5)
}
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
     "Indicates the port status of rr-sat card after the communication
      is broken. When configured to smart-port, the port disables itself
      when there is no management communication with attachment.
      interRptrPort - this port is used to connect an external 
                      repeater. No security can be set for this port.
                      MAC Address is set to 0. No traps 'MAC Address changed'
                      are generated.
      multiConnPort - this port is used for connection of external
                      4 port repeater. 8 securities per port exist.
                      MAC Address is set to 0. No traps 'MAC Address changed'
                      are generated. "
      ::= { prtRREntry 12 }

prtRRAttachment  OBJECT-TYPE
      SYNTAX  INTEGER
{
		none(1),
		station(2),
		sLau2(3),
		sLau4(4)
}
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
     "Indicates the type of the port attachment. Smart attachment types
      are applicable for rr-sat card only"
      ::= { prtRREntry 13 }
    
--
--RADRing specific WAN modules port information 
--

prtMMTable OBJECT-TYPE
      SYNTAX SEQUENCE OF PrtMMEntry
      MAX-ACCESS not-accessible
      STATUS current
      DESCRIPTION
	  "RADRing specific WAN modules port information."
      ::= { prtRR 2 }

prtMMEntry OBJECT-TYPE
      SYNTAX    PrtMMEntry
      MAX-ACCESS    not-accessible
      STATUS    current
      DESCRIPTION
	  "An entry in table prtMMTable."
      INDEX     { prtMMmdlID, prtMMID  }
      ::= { prtMMTable 1 }
 
PrtMMEntry ::=
      SEQUENCE {
	  prtMMmdlID
	     Integer32,
	  prtMMID
	     Integer32,
      prtMMAdminCmd
	     INTEGER,
      prtMMOperStatus
	     INTEGER,
      prtMMTestStatus
	     INTEGER,
      prtMMBertResult
	     INTEGER,
      prtMMTrapMaskEnable
	     INTEGER
      }

prtMMmdlID OBJECT-TYPE  
      SYNTAX  Integer32   
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "A unique identifier of the module(slot no.)this port is attached to."
      ::= { prtMMEntry 1 }

prtMMID OBJECT-TYPE  
      SYNTAX  Integer32   
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "A unique identifier of the port (link) within its module."
      ::= { prtMMEntry 2 }

prtMMAdminCmd  OBJECT-TYPE
      SYNTAX  INTEGER {
	   normal(1),
	   locLoop(2),    
	   remLoop(3)    
}            
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Setting the object to locLoop(2) or remLoop(3) will activate the corresponding test.
	  Value remLoop(3) activates the BERT test on the related link if mdlMMBertCmd
	  is set to on(3). Setting parameter to normal(1), restores the normal operation
	  of the link. The parameter is read-only when value of mdlRRMngState is disable"
      ::= { prtMMEntry 3 }

prtMMOperStatus OBJECT-TYPE
      SYNTAX  INTEGER {
	   operational(1),
	   locSyncLoss(2),    
	   remSyncLoss(3)
}            
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "Reflects the Synchronization status. "
      ::= { prtMMEntry 4 }

prtMMTestStatus OBJECT-TYPE
      SYNTAX  INTEGER {
	   normal(1),
	   locLoop(2),    
	   remLoop(3),
	   bert(4)    
}            
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "Reflects the link status. When the object is set to the value of bert(4),
      it means that the remote loopback is active too. "
      ::= { prtMMEntry 5 }

prtMMBertResult  OBJECT-TYPE
      SYNTAX  INTEGER {
	   unknown(1),
	   fault(2),    
	   ok(3)    
}            
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "The value of this object indicates the status of the last BERT test."
      ::= { prtMMEntry 6 }

prtMMTrapMaskEnable OBJECT-TYPE
	   SYNTAX  INTEGER {
	   off(2),
	   on(3)
      }
      MAX-ACCESS  read-write    
      STATUS  current
      DESCRIPTION
      "When this object is set to OFF by the management console, the 
       agent starts sending the unmasked traps related to this module. 
       Otherwise, no traps are sent by this module."
	   ::= { prtMMEntry 7 }



--
--RADRing specific Ethernet port information
--

prtETTable OBJECT-TYPE
      SYNTAX SEQUENCE OF PrtETEntry
      MAX-ACCESS not-accessible
      STATUS current
      DESCRIPTION
	  "RADRing specific Ethernet port information"
      ::= { prtRR 3 }

prtETEntry OBJECT-TYPE
      SYNTAX    PrtETEntry
      MAX-ACCESS    not-accessible
      STATUS    current
      DESCRIPTION
	  "An entry in table prtETTable."
      INDEX     { prtETmdlID, prtETID }
      ::= { prtETTable 1 }
 
PrtETEntry ::=
      SEQUENCE {
	  prtETmdlID
	     Integer32,
	  prtETID
	     Integer32,
	  prtETTrapMaskEnable
	      INTEGER
		}

prtETmdlID OBJECT-TYPE
      SYNTAX  Integer32   
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "A unique identifier of the module, (slot no.), this port is attached to."
      ::= { prtETEntry 1 }

