      ::= {sysFrameAllocationTable 1}

SysFrameAllocationEntry ::=
      SEQUENCE {
   sysFaConfigIdx   Integer32,
   sysFaLink        INTEGER, 
   sysFaPrt         Integer32,
   sysFaStartingBit Integer32,
   sysFaNumOfBits   Integer32 
	}

sysFaConfigIdx OBJECT-TYPE
      SYNTAX  Integer32 (1..255)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "This object indicates the Configuration number (1..255) 
       referred.
       Value of  255 specifies the TmpCnfg (Scratch).
       Write operation for the table entries is allowed only to 
       Config=255."
      ::= { sysFrameAllocationEntry 1}

sysFaLink OBJECT-TYPE
      SYNTAX INTEGER { 
                    kmxMlA  (103),
                    kmxMlB  (104)
      }
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "The main link on which frame allocation is done."
      ::= { sysFrameAllocationEntry 2}

sysFaPrt OBJECT-TYPE
      SYNTAX Integer32 
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "The Frame Allocation (FA) port.
       The ports will be specified by:
          Slot*100+Ch.
       - for Bypass  of KDI or KDI/CH the port will be:
          Slot*100+11 (internal-1=non-existent port ; just for use
                       of the same MIB table)
       - for all the programmed cards, there will be an artificial
         port entry that will represent the card, in order to 
         specify  the Control Signal Bits that are at card level:
          Slot*100+0 . "
      ::= { sysFrameAllocationEntry 3}

sysFaStartingBit OBJECT-TYPE
      SYNTAX Integer32 (0..239)
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The data starting bit for the channel.
       - for Bypass of KDI or KDI/CH, this will be the Starting Bit 
         of the Bypass Window on the specific sysFaLink.
       - for Control Signals of a card  ( sysFaPrt=Slot*100+0)
         this will be the Starting Bit of the Control Signals of the 
         card."
      ::= { sysFrameAllocationEntry 4}

sysFaNumOfBits OBJECT-TYPE
      SYNTAX Integer32 (0..239)
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The data size in number of bits for the channel.
       - for Bypass of KDI or KDI/CH, this will be the Bypass Window 
         size. In this case, this value must be the same  for both 
   MLs.
       - for Control Signals of a card  ( sysFaPrt=Slot*100+0)
         this will be the number of Control Signal Bits of the 
         card."
      ::= { sysFrameAllocationEntry 5}


