-- Ring information for the ring the agent is connected to.

stkHubTR OBJECT IDENTIFIER ::= { radStkHub 2 }  --  Stackable Token Ring

stkHubTRRing       OBJECT IDENTIFIER ::= { stkHubTR 1}

stkHubTRRingCorrelationEnable OBJECT-TYPE
    SYNTAX  INTEGER {
        off(2),
        on(3)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "When set to ON(3), the in-band agent should perform
       correlation each time a station enters/leaves the segment."
    ::= { stkHubTRRing 1 }

stkHubTRRingAutorecEnable OBJECT-TYPE
    SYNTAX  INTEGER {
        off(2),
        on(3)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "When set to ON(3), the in-band agent should perform
       autorecovery each time a beacon status is detected."
    ::= { stkHubTRRing 2 }

stkHubTRRingAutorecResult OBJECT-TYPE
    SYNTAX  INTEGER {
        not-started(1),
        started(2),
        segment-ok(3),
        segment-fixed(4),
        segment-closed(5),
        failed(6)
    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
       "When autorecovery is completed the result will be recorded.
       While in process, STARTED will be recorded.Before process, NOT STARTED
       will be recorded"
    ::= { stkHubTRRing 3 }


stkHubTRRingResetStatistics OBJECT-TYPE
    SYNTAX  INTEGER {
       unknown(1),
       off(2),
       on(3)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
      "Setting the value of this attribute to ON(3) resets all the inband
      statistics and error counters."
    ::= { stkHubTRRing 4 }

stkHubTRRingLineErrCnt OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
       "This counter is incremented when a frame or token is copied or
       repeated by any of the stations on the ring, the E bit is zero in the frame
       or token and one of the following conditions exists: 1) there is a
       non-data bit (J or K bit) between the SD and the ED of the frame or token, or
       2) there is an FCS error in the frame."
    ::= { stkHubTRRing 5 }

stkHubTRRingBurstErrCnt OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
          "This counter is incremented when any of the stations on the ring detects the
          absence of transitions for five half-bit timers (burst-five error)."
    ::= { stkHubTRRing 6 }

stkHubTRRingACFCErrCnt  OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "This counter is incremented when any of the stations on the ring receives an 
        AMP or SMP frame in which A is equal to C is equal to 0, and then receives
        another SMP frame with A is equal to C is equal to 0 without first receiving an AMP
        frame. It denotes a station that cannot set the AC bits properly."
    ::= { stkHubTRRing 7 }

stkHubTRRingTotErrCnt OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The total of the above described errors"
    ::= { stkHubTRRing 8 }

stkHubTRRingTotErrThreshold OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "The Total Errors Threshold value. A trap indicating Soft Error
	    will be sent, if the total number of errors during the last 10 seconds,
		reached the threshold value. Threshold value = 0 , disables this trap."
    ::= { stkHubTRRing 9}

stkHubTRRingTotalFrames OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The total frames of the Ring."
    ::= { stkHubTRRing 10 }

stkHubTRRingTotalFramesRate OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "The rate of total frames on the Ring."
    ::= { stkHubTRRing 11 }

stkHubTRRingTotalOctets OBJECT-TYPE
    SYNTAX  Counter32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The total octets of the Ring."
    ::= { stkHubTRRing 12 }

stkHubTRRingOctetsRatio OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
    "Current traffic for the last time interval. The traffic is 
     expressed in percents of the maximum TokenRing traffic ."
    ::= { stkHubTRRing 13 }



--  Bridge Hub Module table for TR media type
--  Contains general information about the Module


stkHubTRMdl       OBJECT IDENTIFIER ::= { stkHubTR 2}

 stkHubTRMdlTable OBJECT-TYPE
    SYNTAX SEQUENCE OF  StkHubTRMdlEntry
    MAX-ACCESS not-accessible
    STATUS current
	DESCRIPTION
	"Bridge Hub Module table for TR media type
	 Contains general information about the Module"
    ::= { stkHubTRMdl 1 }

stkHubTRMdlEntry OBJECT-TYPE
    SYNTAX StkHubTRMdlEntry
    MAX-ACCESS    not-accessible
    STATUS current
	DESCRIPTION
	"An entry in table stkHubTRMdlTable."
    INDEX     {stkHubTRMdlID}
    ::= { stkHubTRMdlTable 1}

StkHubTRMdlEntry ::=
    SEQUENCE {
   stkHubTRMdlID
           Integer32,
   stkHubTRMdlType
          INTEGER,
   stkHubTRMdlReset
          INTEGER,
   stkHubTRMdlLed
          OCTET STRING,
   stkHubTRMdlSpeed
          INTEGER,
   stkHubTRMdlMngState
          INTEGER,
   stkHubTRMdlAutodisable
          INTEGER,
   stkHubTRMdlConfigMsg
          OCTET STRING
        }

stkHubTRMdlID OBJECT-TYPE
      SYNTAX  Integer32 (1..10)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "A unique identifier of the module in stack. Internal module identifier
      is always 1 "
      ::= {stkHubTRMdlEntry 1}

stkHubTRMdlType OBJECT-TYPE
      SYNTAX  INTEGER {
	       unknown(1),
		   sah16int(2),
		   sah16ext(3),
		   empty(255)
		      }
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
       "Indicates the type of the module connected with specific identifier "
      ::= {stkHubTRMdlEntry 2 }

 stkHubTRMdlReset  OBJECT-TYPE
      SYNTAX  INTEGER {
           unknown(1),
           off(2),
           on(3)
             }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "When this object is set to the value of ON(3),
       the module performs software reset. Upon completion, a trap  will
       be issued to the management station and  the value
       will be set to OFF."
      ::= {stkHubTRMdlEntry 3}

stkHubTRMdlLed  OBJECT-TYPE
      SYNTAX  OCTET STRING
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
        "This entry specifies  the LEDs status in the module . Each led will 
         be represented by 4 bits where the first 2 bits will specify  the 
         status (NA, OFF, ON, BLINK), and the next two bits
         will specify the color (RED, GREEN, YELLOW)"
      ::= {stkHubTRMdlEntry 4}

stkHubTRMdlSpeed OBJECT-TYPE
      SYNTAX  INTEGER {
          unknown(1),
          fourMegabit(2),
          sixteenMegabit(3)
      }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Attribute describing the bandwidth of the media as set by the module
       switch.       
       For certain products, if the value of stkHubTRMdlMngState 
       is 'enabled', then stkHubTRMdlSpeed can be set to the desired 
       value. Else, it will be read-only, and will show the status 
       of the jumper set of the main module.
       Refer product's manual."
      ::= { stkHubTRMdlEntry 5}

stkHubTRMdlMngState   OBJECT-TYPE
      SYNTAX  INTEGER {
	   unknown (1),
	   disabled(2),
       enabled (3)
      }    
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
       "This object indicates the status of the Manage switch 
        as set on the back panel of the main module.
        'enabled' - means that management can override setting of 
                    some HW switch/es."
      ::= {stkHubTRMdlEntry 6 }


stkHubTRMdlAutodisable  OBJECT-TYPE
      SYNTAX   INTEGER {
                unknown(1),
                off(2),
                on(3)
        }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Attribute describing the status of port Autodisable 
       mechanism, that protects against inserting in Token Ring 
       adapters at the wrong speed.
       For certain products, if the value of stkHubTRMdlMngState 
       is 'enabled', then stkHubTRMdlAutodisable can be set to 
       the desired value. Else, it will be read-only, and will 
       show the status of the jumper set of the main module.
       Refer product's manual."
   ::= {stkHubTRMdlEntry 7 }


stkHubTRMdlConfigMsg OBJECT-TYPE
      SYNTAX  OCTET STRING
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Full configuration message  coded in internal RND application format.
      This entry describes  module' configuration. "
      ::= {stkHubTRMdlEntry 8}


