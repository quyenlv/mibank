trimIfBaudRate    OBJECT-TYPE          -- Level II routing
    SYNTAX   Integer32   -- word
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	" Defines the baud rate in use by the interface. The valid
	 rates are determined by the type of the media for which this
	 baud rate is defined."
    ::= { trimIfEntry 6  }


trimIfCost    OBJECT-TYPE          -- Level II routing
    SYNTAX   Integer32   -- word 
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"This object is inapplicable to Trimbridge."
    ::= { trimIfEntry 7  }


trimIfCompression   OBJECT-TYPE
    SYNTAX  INTEGER {
       enable(1),
       disable(2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
       "Defines whether data compression is enabled or disabled for the 
       interface."
    ::= {  trimIfEntry 8  }


  trimIfCompressionStatus  OBJECT-TYPE
  SYNTAX  INTEGER {
       not-inserted(1),
       active(2),
       not-active(3),
       disable(4)
    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	  "Defines the status of the compression component: 
	  not-inserted - compression component is not in the device
	  active - compression is enabled and operates correctly
	  not-active - compression is enables but does not operate. 
		       The reason may be that there is no 
		       compression component on the device connected 
		       to this interface.
	  disabled - compression was disabled by management, using 
		       variable trimCompression."
    ::= {  trimIfEntry 9  }
  

  trimIfCompressionRate  OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	  "Percentage of compression. This variable applies only 
	  if trimCompressionStatus is active."
    ::= {  trimIfEntry 10  }


  trimIfLATCompression  OBJECT-TYPE
    SYNTAX  INTEGER {
       enable(1),
       disable(2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	  "Enable/Disable the LAT Compression." 
    ::= {  trimIfEntry 11  }


  trimIfCompressionType  OBJECT-TYPE
    SYNTAX  INTEGER {
       none(1),
       lowSpeed(2),
       highSpeed(3)
    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	  "This parameter indicates the type of compression option available in
	   the device. 
	     `None(1)           - indicates that there is no compression option in the device
	      lowSpeed(2)   - indicates a compression option which is  operational up 
					 to line speed sof 64 kbps.
	      highSpeed(3) - indicates a compression option which is  operational up
					 to line speeds of 2 Mbps."
    ::= {  trimIfEntry 12  }



trimIfFilterMode  OBJECT-TYPE
    SYNTAX  INTEGER {
       destinationOnly(1),
       sourceAndDestination(2)
    }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"This parameter indicates the type of filtering in the device. "
    ::= {  trimIfEntry 13 }

trimIfConType  OBJECT-TYPE
    SYNTAX  INTEGER {
       unknown(1),
	   none(2),
       x21Old(3),
	   x21(4),
	   v35Old(5),
	   v35(6),
	   rs530Old(7),
	   rs530(8),
	   rs232Old(9),
	   rs232(10),
	   spsV35(11),
	   spsRS530(12),
	   spsRS232(13),
	   spsX21(14),
	   isdn(15)
    }
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"This parameter indicates the connector type of an interface. "
    ::= {  trimIfEntry 14 }



trimIfRemAddress OBJECT-TYPE
    SYNTAX   DisplayString (SIZE(1..17))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"         "
    ::= {  trimIfEntry 15  }



trimIfLocSubAddress OBJECT-TYPE
    SYNTAX   DisplayString (SIZE(1..6))
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"Space is not valid. If the string includes a space,
