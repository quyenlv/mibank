    ::=  { pktSwchLapbSTable 1  }

PktSwchLapbSEntry ::= SEQUENCE {
    pktSwchLapbLinkNumberS
     Integer32,
    pktSwchLapbInternalClockS
     INTEGER,
    pktSwchLapbLineOptionS
     Integer32,
    pktSwchLapbSegmentSizeS
     Integer32,
    pktSwchLapbXIDS
     Integer32,
    pktSwchLapbAckCounterS
     Integer32,
    pktSwchLapbAckTimerS
     Integer32
      }

pktSwchLapbLinkNumberS  OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The link number with which this entry is associated.  This value
	 corresponds to pktSwchPhyInterfaceNumber.  There is an entry for
	 each link for which the pktSwchPhyInterfaceProtocol is X.25  and one
	 entry for each link and DLCI if the pktSwchPhyInterfaceProtocol for
	 the link is Frame Relay and the DLCI is defined as using X.25."
    ::= { pktSwchLapbSEntry 1 }

pktSwchLapbInternalClockS  OBJECT-TYPE
    SYNTAX INTEGER {
       r2400bps (1),
       r4800bps (2),
       r9600bps (3),
       r14400bps(4),
       r19200bps(5),
       r38400bps(6),
	   r48Kbps  (7),
       r56Kbps  (8),
       r64Kbps  (9),
       r128Kbps (10),
       r256Kbps (11),
	   r384Kbps (12),
       r504Kbps (13),
	   r768Kbps (14),
	   r1008Kbps(15),
       r1466Kbps(16),
       r2016Kbps(17),
       external(255)
     }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"The internal clock speed to be used on this LAPB link.  If external clock is used, the value is
	 255."
    ::= { pktSwchLapbSEntry 2 }

pktSwchLapbLineOptionS  OBJECT-TYPE
    SYNTAX Integer32 (0..2048)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"The line option to be used on this link.  A value of zero indicates
	 no options.  A value between 0 and 255 inclusive specifes a time in
	 minutes after which the line will be disconnected if no session is open.
	 The following values have the associated meanings:
			       256     Dialed line
			       512     Return to leased line
			       1024   DTR is ON permanent
			       2048   DTR is OFF permanent.
		 any valid combination of the above options should be represented as
		 a sum of the corresponding values. Valid combinations are:
		 DTR ON or OFF (but not both). Dialed line options or not applicable.
		 If DTR control is on (256), then all combinations are possible
		 except 1024 and 2048 (which are not possible)."
    ::= { pktSwchLapbSEntry 3 }

pktSwchLapbSegmentSizeS  OBJECT-TYPE
    SYNTAX Integer32 (1..4096)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"This value is for the use of billing information.  The value can be no larger than the maximum
		packet size."
    ::= { pktSwchLapbSEntry 4 }


pktSwchLapbXIDS  OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS read-write
    STATUS  current
    DESCRIPTION
	"When this parameter is different than zero, this lapb is configured
	 as a V25 bis dialed link. This is the XID entry Index which this dialed link is
	 associated with."
	::= { pktSwchLapbSEntry 5 }

pktSwchLapbAckCounterS  OBJECT-TYPE
    SYNTAX  Integer32 (1..128)
    MAX-ACCESS read-write
    STATUS  current
    DESCRIPTION
        "This object defines how many frames will be received on the link
         before the PS will send Acknowledge (RR frame).
         It is used for Piggy Back Acknowledge (ack within the info frames)
         and saves traffic on the line.
         Default value: 1 ."
    ::= { pktSwchLapbSEntry 6 }

pktSwchLapbAckTimerS  OBJECT-TYPE
    SYNTAX  Integer32 (0..255)
    MAX-ACCESS read-write
    STATUS  current
    DESCRIPTION
         "The Ack Timer defines the maximum time in seconds that the device
         will wait before sending RR acknowledgment on the received frames.
         It is used for Piggy Back Acknowledge (ack within the info frames)
         and saves traffic on the line.
         Default value: 0 ."
        ::= { pktSwchLapbSEntry 7 }

