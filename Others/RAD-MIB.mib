modemStatistics OBJECT IDENTIFIER  ::=  {modem 3}

modemGeneralTable  OBJECT-TYPE
      SYNTAX SEQUENCE OF ModemGeneralEntry
      MAX-ACCESS not-accessible
      STATUS  current
      DESCRIPTION
      "The IMOD General data Table. The data is specific to each 
       modem."
       ::= { modemStatistics 1 }

modemGeneralEntry OBJECT-TYPE
      SYNTAX ModemGeneralEntry
      MAX-ACCESS not-accessible
      STATUS current
      DESCRIPTION
      "An entry in the modemGeneralTable. This entry is per modem. 
       Each modem has a specific index, which identifies it."
      INDEX {modemIdx}
      ::= { modemGeneralTable 1}

ModemGeneralEntry ::=
      SEQUENCE {
	  modemCurrentElapsedTime Integer32,
	  modemValidIntervals     Integer32
      }


modemCurrentElapsedTime	 OBJECT-TYPE
	  SYNTAX  Integer32 (0..899)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "The number of seconds that have elapsed since the beginning
	   of the current error-measurement period."
	  ::= {modemGeneralEntry 1}

modemValidIntervals	 OBJECT-TYPE
	  SYNTAX  Integer32 (0..96)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "The number of previous intervals for which data was 
	   collected.
	   The value will be 96 unless the interface was brought on-line 
	   within the last 24 hours, in which case the value will be the 
	   number of complete 15 minute intervals since the interface 
	   has been online."
	  ::= {modemGeneralEntry 2}


modemIntervalTable  OBJECT-TYPE
      SYNTAX SEQUENCE OF ModemIntervalEntry
      MAX-ACCESS not-accessible
      STATUS  current
      DESCRIPTION
      "The IMOD Interval Table. The data is specific to each 
       modem."
       ::= { modemStatistics 2 }

modemIntervalEntry OBJECT-TYPE
      SYNTAX ModemIntervalEntry
      MAX-ACCESS not-accessible
      STATUS current
      DESCRIPTION
      "An entry in the modemIntervalTable. This entry is per modem 
       and per interval selected." 
      INDEX {modemIdx, modemIntervalNumber}
      ::= { modemIntervalTable 1}

ModemIntervalEntry ::=
      SEQUENCE {
	  modemIntervalNumber     Integer32,
	  modemSyncLoss           Integer32,
	  modemLineQuality        INTEGER,
	  modemLineUtility        Integer32,
	  modemRTS                Integer32,
	  modemCD                 Integer32,
	  modemBertResult   DisplayString,
	  modemBpvError      OCTET STRING,
	  modemFoError       OCTET STRING
      }

modemIntervalNumber	 OBJECT-TYPE
	  SYNTAX  Integer32 (1..96)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "The number between 1 and 96, where 1 is the most recently 
	   completed 15 minute interval and 96 is the least recently 
	   completed 15 minutes interval (assuming that all 96 
	   intervals are valid). Maximum valid number is 
	   modemValidIntervals."
	  ::= {modemIntervalEntry 1}

modemSyncLoss	 OBJECT-TYPE
	  SYNTAX  Integer32 (0..101)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "Percent of interval, SyncLoss signal is ON.
	   101 = notApplicable/unknown. "
	  ::= {modemIntervalEntry 2}

modemLineQuality   OBJECT-TYPE
	  SYNTAX  INTEGER
	      {
		   notApplicable (1),
		   bad  (2),
		   mid  (3),
		   good (4),
		   unknown (5),
		   syncLoss(6)
	      }
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Quality of Line for the selected modem, in the selected valid 
       interval."
	  ::= {modemIntervalEntry 3}

modemLineUtility	 OBJECT-TYPE
	  SYNTAX  Integer32 (0..101)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "Percent of interval, Line is utilized.
	   101 = notApplicable/unknown. "
	  ::= {modemIntervalEntry 4}

modemRTS	 OBJECT-TYPE
	  SYNTAX  Integer32 (0..101)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "Percent of interval, RTS signal is ON.
	   101 = notApplicable/unknown. "
	  ::= {modemIntervalEntry 5}

modemCD	 OBJECT-TYPE
	  SYNTAX  Integer32 (0..101)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "Percent of interval, CD signal is ON.
	   101 = notApplicable/unknown. "
	  ::= {modemIntervalEntry 6}

modemBertResult	 OBJECT-TYPE
	  SYNTAX  DisplayString 
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
	  "Result of BERT for the selected modem, in the selected 
	   valid interval."
	  ::= {modemIntervalEntry 7}

modemBpvError OBJECT-TYPE
      SYNTAX OCTET STRING (SIZE(0..1)) 
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "BPV Error.
       The Octet will be built as follows:
       MSB 7  6  5  4  3  2  1  0 LSB
           |__|  |______________|
         y=0..3      x=0..63
         
         
         BPV Error = x * (10**y)
         
       An Agent that does not support this object will send Size=0."
      ::= {modemIntervalEntry 8}

modemFoError OBJECT-TYPE
      SYNTAX OCTET STRING (SIZE(0..1)) 
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "FO Error.
       The Octet will be built as follows:
       MSB 7  6  5  4  3  2  1  0 LSB
           |__|  |______________|
         y=0..3      x=0..63
         
         
         FO Error = x * (10**y)
         
       An Agent that does not support this object will send Size=0."
      ::= {modemIntervalEntry 9}



