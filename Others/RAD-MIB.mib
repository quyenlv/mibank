SysGenKmuxEntry ::=
      SEQUENCE {
   sysGenConfigIdx              Integer32,
   sysGenFlipIntegration        INTEGER,
   sysGenMlMode                 INTEGER,
   sysGenFaType                 INTEGER,
   sysGenLinkRecovery           INTEGER,
   sysGenLinkSourceClock        INTEGER,
   sysGenMngBwA                 Integer32,
   sysGenMngBwB                 Integer32,
   sysGenAutoDialSlot           Integer32,
