	  modmPrtHdslIndex		     Integer32,
	  modmPrtHdslDedicatedTs     Integer32,
	  modmPrtHdslPriorityBumping OCTET STRING,
      modmPrtHdslMngProtocol     INTEGER, 
      modmPrtHdslRoutProtocol    INTEGER, 
      modmPrtHdslInbandMngRate 	 INTEGER
	  }

