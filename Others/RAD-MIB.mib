stkHubTRPrtConnectorType OBJECT-TYPE
      SYNTAX  INTEGER {
           notApplicable(1),
           idc(2),
           rj45(3),
           srJ45(4),
           db9(5),
           bnc(6),
           fo-st(7),
           db25(8)
--         fo-sma(8),
--         fo-fc(9)
      }
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Type of hardware connector in the port."
      ::= { stkHubTRPrtEntry 4 }

stkHubTRPrtAdminCmd OBJECT-TYPE
      SYNTAX  INTEGER {
          unknown(1),
          disable(2),
          enable(3),
          short(4),
          normal(5)
      }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
         "By setting this attribute to DISABLE(2) or SHORT(4) the
         management station can force this  lobe/port-connection  to be
         removed and block any access to the  network via this lobe/port.
         It is not mandatory that a certain lobe/port will support all the
         enumerated states. The state of the lobe/port is type specific and
         indicated within the specific group"
      ::= { stkHubTRPrtEntry 5 }

stkHubTRPrtActiveState OBJECT-TYPE
      SYNTAX  INTEGER {
          unknown(1),
          off(2),
          on(3),
          standby(4)
      }
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
        "This attribute describes the electrical or optical activity
         state of  a lobe or the state of a port.  Certain values may
         not be applicable in some cases"
      ::= { stkHubTRPrtEntry 6 }

stkHubTRPrtTrapMaskEnable OBJECT-TYPE
      SYNTAX  INTEGER {
          off(2),
          on(3)
      }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "When this attribute is set to OFF by the management console the
       agent starts sending the unmasked traps related to this port.
       Otherwise no traps from this port are sent."
      ::= { stkHubTRPrtEntry 7 }

stkHubTRPrtStatus OBJECT-TYPE
      SYNTAX  INTEGER {
          unknown(1),
          disabledManual(2),
          disabledAutorec(3),
          disabledSecurity(4),
          disabledAuto(5),
          disabledFlip(6),
          inserted(7),
          bypass(8),
          forcedloop(9),
          trunkfail(10),
          normal(11)
      }
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "This attribute indicates the status of this lobe/port connection:
      DISABLED-(2) to  (6) indicates the reason for the disabled state.
      INSERTED(7)indicates that the lobe hardware inserts this lobe into
      the ring , thus it becomes part of the ring
      BYPASSED(8)indicates that the ring bypasses this lobe and the lobe
      hardware provides loopback connection towards the station for media 
      test purposes.
      FORCEDLOOP(9) indicates the port is in a loopback state and the
      main ring is connected to the backup path.
      TRUNKFAIL (10) indicates that the ring connection is broken for a
      repeater, converter or a cable protector.
      NORMAL(11) indicates the port is in normal condition
      It is not mandatory that a certain lobe/port will support all the 
       enumerated states."
      ::= { stkHubTRPrtEntry 8 }

stkHubTRPrtCableProtectEnable OBJECT-TYPE
      SYNTAX  INTEGER {
          unknown(1),
          off(2),
          on(3),
          inhibit(4)
      }
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "The value of this attribute determines activation of the cable-fault
      protection mechanism of  TCR and the optical link mechanism of
      TFR and TFC ."
      ::= { stkHubTRPrtEntry  9 }

stkHubTRPrtFlipProtectEnable OBJECT-TYPE
      SYNTAX  INTEGER {
          unknown(1),
          off(2),
          on(3)
      }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "The value of this attribute determines activation of the flipping-lobe
      protection mechanism of the port."
      ::= { stkHubTRPrtEntry  10 }

stkHubTRPrtMACAddress OBJECT-TYPE
      SYNTAX  MacAddress
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      " The value of this object is the specific MAC address
      of this interface which uniquely identifies this Interface
      on a ring."
      ::= { stkHubTRPrtEntry 11 }

stkHubTRPrtType OBJECT-TYPE
      SYNTAX  INTEGER
      {
      notApplicable(1),
      tfr(2),
      tfc(3),
      tcr(4),
      tcp(5)
      }
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Type of the port specific hardware"
      ::= { stkHubTRPrtEntry 12}

		
