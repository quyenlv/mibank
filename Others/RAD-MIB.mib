--
-- Segments 
--
segTRTable OBJECT-TYPE
               SYNTAX SEQUENCE OF SegTREntry
               MAX-ACCESS not-accessible
               STATUS current
			   DESCRIPTION
			   "Segments."
               ::= { segTR 1}

segTREntry OBJECT-TYPE
               SYNTAX SegTREntry
               MAX-ACCESS not-accessible
               STATUS current
			   DESCRIPTION
			   "An entry in table segTRTable."
               INDEX { segTRID}
               ::= { segTRTable 1}
   
SegTREntry ::= 
            SEQUENCE {
               segTRID
                  Integer32,
               segTRLastSlot
                  Integer32,
               segTRFirstSlot
                  Integer32,
               segTRStatus
                  INTEGER
                 }

segTRID OBJECT-TYPE
           SYNTAX  Integer32  (1..20) 
           MAX-ACCESS  read-only
           STATUS  current
           DESCRIPTION
           "Segment unique identification number (internal). It is  
	        the extreme left slot, marking the beginning of the 
	        segment."
           ::= {  segTREntry 1 }

segTRLastSlot OBJECT-TYPE  
           SYNTAX  Integer32   
           MAX-ACCESS  read-only
           STATUS  current
           DESCRIPTION
           "Location of the right most slot marking the end of the 
            segment. In the RADring hub it is slot number 20 or the slot 
            where end of segment module is inserted or the slot before
            a beginning of a new segment. ( End of segment 
            module could be one with termination on or TIO, TFR, TCR etc.)"
           ::= {  segTREntry 2 }

segTRFirstSlot OBJECT-TYPE
           SYNTAX  Integer32     
           MAX-ACCESS  read-only
           STATUS  current
           DESCRIPTION
           "Location of the left most slot marking the beginning
           of the segment. In the RADring hub it is slot number 1 or 
           the slot number following the end of segment module, or where
           a repeater or TIO is inserted."
           ::= {  segTREntry 3 }

segTRStatus OBJECT-TYPE 
      SYNTAX  INTEGER {
        unknown(1),
        beaconing(2),
        normal(3)
	 }   
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Describes the status of the segment :
      Normal(3) - The segment is operating normally
      Beaconing(2) - The ring is not operative; no data is transmitted.
      Segment without inband module within its limit will display Unknown(1).
       This entry is specially usefull when the hub is configured with multiple segments
        and each segment is part of another net"
      ::= { segTREntry 4 }


