-- System Priorities

pktSwchPriority  OBJECT IDENTIFIER ::= { pktSwchSystem 7 }

-- This is a table of Tx Priorities.
-- At present, it is used for DLCIs. There are 4 levels of priorities.
-- Each level contains 2 parameters: max delay time and max queue length.

-- This is a status which is used for saving the Tx Priority table.
pktSwchTxPriorityStatus OBJECT-TYPE
    SYNTAX  INTEGER {
       valid(1),
       underCreation(3)
     }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
    "The status of the Priorities table.
     Used for saving the Priority parameters. "
         ::= {pktSwchPriority 1}

pktSwchTxPriorityTable    OBJECT-TYPE
    SYNTAX  SEQUENCE  OF PktSwchTxPriorityEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The Tx Priorities table."
         ::= {pktSwchPriority 2}

pktSwchTxPriorityEntry OBJECT-TYPE
    SYNTAX  PktSwchTxPriorityEntry
    MAX-ACCESS  not-accessible
    STATUS  current
    DESCRIPTION
        "The Tx Priority Entry defines the max delay time and max queue
         length for each Priority level."
    INDEX   {pktSwchTxPriorityLevel}
    ::=  { pktSwchTxPriorityTable 1 }

PktSwchTxPriorityEntry ::= SEQUENCE {
pktSwchTxPriorityLevel
Integer32,
pktSwchTxPriorityMaxDelayTime
Integer32,
pktSwchTxPriorityMaxQueueLen
Integer32     }

pktSwchTxPriorityLevel  OBJECT-TYPE
    SYNTAX Integer32 (1..4)
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
        "Level of an entry in the Tx Priority Table.
         At present, the PS supports 4 priority levels.
         Default level: 1"
    ::= { pktSwchTxPriorityEntry 1 }

pktSwchTxPriorityMaxDelayTime  OBJECT-TYPE
    SYNTAX Integer32 (0..60000)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Max delay time in Tx queue (in 100 msec intervals).
         Each frame will wait no more than the Max delay time until
         it is handled.
         0 means that the delay is Unlimited.
         Default value: 0"
    ::= { pktSwchTxPriorityEntry 2 }


pktSwchTxPriorityMaxQueueLen  OBJECT-TYPE
    SYNTAX Integer32 (0..65535)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
        "Max allowed length of the Tx queue. Frames will be discarded
         if the length is exceeded.
         0 means that the queue is of Unlimited length.
         Default value: 0"
    ::= { pktSwchTxPriorityEntry 3 }


