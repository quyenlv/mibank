-- trimBackup group defines the parameters for the backup feature

trimBackup    OBJECT IDENTIFIER ::= { trim 2 }

trimBackupState OBJECT-TYPE
    SYNTAX  INTEGER {
      main(1),
      backup(2)
    }
    MAX-ACCESS  read-only
    STATUS current 
    DESCRIPTION
	"If main - the bridge is connected via the main physical link. 
	If backup - the automatic backup feature was enabled 
	and the bridge is connected via the backup physical link."
    ::= { trimBackup 1 }

   trimDialUpStatus  OBJECT-TYPE
    SYNTAX  INTEGER {
      enable(1),
      disable(2)
      }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"This parameter controls the operation of dial-up backup function.
	  If status is disable(2), dial-up backup will not be initiated when the 
	  main link fails or exceeds the specified error limit.
	   If status is enable(1), the dial-up backup will be initiated if the main
	  link fails or if the error rate on the main link exceeds the limit specified
	  by variable trimDialUp ErrorThreshold."
    ::= { trimBackup 2 }

trimBackLinkType  OBJECT-TYPE
    SYNTAX  INTEGER {
	dialUp(1),
	leased (2)
	}
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"This variable specifies the type of the backup link. If the type is leased(2), 
	  Trimbridge will reroute traffic to the backup link, when main link fails.
	  If the value is dialUp(1), then the backup dial-up procedure is initiated
	  when the main link fails."
    ::= { trimBackup 3 }



trimDialUpMode  OBJECT-TYPE
    SYNTAX  INTEGER {
      originate(1),
      answer(2)
      }
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"If the mode is originate(1), Trimbridge initiates the backup procedure when the
	  main link fails. if the mode is answer(2), it expects that the bridge on the 
	 other end of the link will initiate the backup procedure. Note that only one 
	 bridge in a pair should be set to mode originate(1). "
    ::= { trimBackup 4 }

trimDialUpErrorThreshold  OBJECT-TYPE
    SYNTAX  INTEGER {
	none(1),
	low(2),
	medium(3),
	high(4)
	    }
    MAX-ACCESS   read-write
     STATUS  current
    DESCRIPTION
	"Error rate on main link at which Backup will be enabled.
	    low    - 1*10-5
	    medium - 5*10-4
	    high   - 1*10-3 
	  If none(1) is selected, then backup is not invoked when there are errors
	  on the main link, but only when the main link is down."
    ::= { trimBackup 5 }

trimDialUpRedialTimeout  OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	  "If an attempt to dial up failed for the specified time, a new redial attempt
	    will be initiated."
       ::= { trimBackup 6}

trimDialUpRedialNum   OBJECT-TYPE
    SYNTAX  Integer32
    MAX-ACCESS   read-write
    STATUS  current
    DESCRIPTION
	"This variable specifies the number of times to initiate the dial-up procedure.
	  If the number of redials is set to zero (0), then the number of redials will be 
	  unlimited. Any other value will cause the dial-up procedure to terminate after 
	 the specified number of re-dial attempts fail. Resetting this variable to any value
	  will re-initiate the dial-up procedure."
       ::= { trimBackup 7 }

trimBackUpConnectDelay OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
     "The connect delay defines the minimum time period that the link must be disconnected."
	::= { trimBackup 8 }

trimBackUpDisConnectDelay OBJECT-TYPE
	SYNTAX  Integer32
	MAX-ACCESS  read-write
	STATUS  current
	DESCRIPTION
		"The disconnect delay defines the delay between the last valid traffic trigger
		  and link disconnection."
	::= { trimBackup 9 }


trimBackUpScheduleTable OBJECT-TYPE
		SYNTAX  SEQUENCE  OF TrimBackUpEntry
		MAX-ACCESS  not-accessible
		STATUS  current
		DESCRIPTION
		       "This is a table that contains the reroute shedule for the backup link.  The index 
			is the day of the week, starting with 1 for Sunday, ending with 7 for 
			Saturday."
	       ::=  { trimBackup 10 }

trimBackUpEntry    OBJECT-TYPE
		SYNTAX  TrimBackUpEntry
		MAX-ACCESS  not-accessible
		STATUS  current
		DESCRIPTION
		"An entry in table trimBackUpScheduleTable."
		INDEX  { trimBackUpDay }
		::=  { trimBackUpScheduleTable 1 }

TrimBackUpEntry ::= SEQUENCE {
		 trimBackUpDay
		 Integer32,
		 trimBackUpFromTo
		 DisplayString
				      }

trimBackUpDay OBJECT-TYPE
		SYNTAX   Integer32
		MAX-ACCESS  read-only
		STATUS  current
		DESCRIPTION
		       "The day of the week for which the entry applies.  The days are numbered from 
		1 to  7, representing Monday through Sunday."
