    INDEX   { pktSwchPlpLinkNumberS }
    ::=  { pktSwchPlpSTable 1  }

PktSwchPlpSEntry ::= SEQUENCE {
    pktSwchPlpLinkNumberS
     Integer32,
    pktSwchPlpInCallOptionS
     Integer32,
    pktSwchPlpOutCallOptionS
     Integer32,
    pktSwchPlpCugSubscriptionS
     INTEGER,
	pktSwchPlpNUIS
	 Integer32,
	pktSwchPlpCpyPktSize
	 Integer32,
	pktSwchPlpCpyWinSize
	 Integer32
    }

pktSwchPlpLinkNumberS  OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS  read-only
    STATUS  current
    DESCRIPTION
	"The ifIndex of the X.25 interface."
    ::= { pktSwchPlpSEntry 1 }

pktSwchPlpInCallOptionS  OBJECT-TYPE
    SYNTAX Integer32 (0..15)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"The in call options for the link.  The value is a sum of values (powers of two), where each
		 value in the sum represents an option.  The values that may be included in the sum and
		 their meanings are:
		0 No options
		1 Fast select acceptance - included in RFC 1382
		2 Reverse charge acceptance - included in RFC 1382
		4 Subaddress in Call User Data field (CUD)
				8 Protocol ID (PID) is not used."
    ::= { pktSwchPlpSEntry 2 }

pktSwchPlpOutCallOptionS  OBJECT-TYPE
    SYNTAX Integer32 (0..15)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
	"The out call options for the link. The value is a sum of values
	 (powers of two), where each value in the sum represents an
	 option. The values that may be included in the sum and their
	 meanings are:
		0 No options
		1 Don't add calling and call addressin call accept packet
		2 Network link, call request spread enabled
		4 Add source link number to called address
	  	8 Don't change calling address
	  	XX - Any combination of the values specified above."
      ::= { pktSwchPlpSEntry 3 }

