stkHubET OBJECT IDENTIFIER ::= { radStkHub 3 }  --  Stackable Ethernet


--  Bridge Hub Module table for ET media type
--  Contains general information about the Module


stkHubETMdl       OBJECT IDENTIFIER ::= { stkHubET 2}

 stkHubETMdlTable OBJECT-TYPE
    SYNTAX SEQUENCE OF  StkHubETMdlEntry
    MAX-ACCESS not-accessible
    STATUS current
	DESCRIPTION
	"Bridge Hub Module table for ET media type
	 Contains general information about the Module"
    ::= { stkHubETMdl 1 }

stkHubETMdlEntry OBJECT-TYPE
    SYNTAX StkHubETMdlEntry
    MAX-ACCESS    not-accessible
    STATUS current
	DESCRIPTION
	"An entry in table stkHubETMdlTable."
    INDEX     {stkHubETMdlID}
    ::= { stkHubETMdlTable 1}

StkHubETMdlEntry ::=
    SEQUENCE {
   stkHubETMdlID
           Integer32,
   stkHubETMdlLed
           OCTET STRING,
   stkHubETMdlReset
          INTEGER
   }


stkHubETMdlID OBJECT-TYPE
      SYNTAX  Integer32 (1..10)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "A unique identifier of the module in stack. Internal module identifier
      is always 1 "
      ::= {stkHubETMdlEntry 1}


stkHubETMdlLed  OBJECT-TYPE
      SYNTAX  OCTET STRING
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
        "This entry specifies  the LEDs status in the module . Each led will 
         be represented by 4 bits where the first 2 bits will specify  the 
         status (NA, OFF, ON, BLINK), and the next two bits
         will specify the color (RED, GREEN, YELLOW)"
      ::= {stkHubETMdlEntry 2}


stkHubETMdlReset  OBJECT-TYPE
      SYNTAX  INTEGER {
           unknown(1),
           off(2),
           hwReset(3),
		   configReset(4)
             }
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "When this object is set to the value of hwReset(3),
       the module performs HW reset. 
	   When this object is set to the value of configReset(4),
       the module performs Configuration reset.
       Upon completion, a trap  will
       be issued to the management station and  the value
       will be set to OFF."
      ::= {stkHubETMdlEntry 3}


