     Integer32,
    pktSwchAsyncRunProChannelNumber
     Integer32,
	pktSwchAsyncRunProRecallChar
	 Integer32,
	pktSwchAsyncRunProEcho
	 INTEGER,
	pktSwchAsyncRunProDataForwChar
	 Integer32,
	pktSwchAsyncRunProIdleTimer
	 Integer32,
	pktSwchAsyncRunProDevFlowCont
	 INTEGER,
	pktSwchAsyncRunProServSig
	 INTEGER,
