pktSwchIsdn       OBJECT IDENTIFIER ::= { radPS 14}

--  Packet Switching devices with Integrated Services Digital Network.

pktSwchIsdnGlobal  OBJECT IDENTIFIER ::= { pktSwchIsdn 1 }

--  ISDN Global Parameters

pktSwchIsdnGlobalCallRetries OBJECT-TYPE
    SYNTAX Integer32 (0..255)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
    "Number of times the call request is repeated after failure.
     Possible values:
     0     - Disable retry mechanism
     1-255 - Number of retries
     default value: 0"
     ::= {pktSwchIsdnGlobal 1}

pktSwchIsdnGlobalCallRetryDelay OBJECT-TYPE
    SYNTAX Integer32 (5..250)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
    "Time interval between retries (in seconds).
     Possible values:
     5 - 250
     Default value: 15"
     ::= {pktSwchIsdnGlobal 2}

pktSwchIsdnGlobalCallinactivity OBJECT-TYPE
    SYNTAX Integer32 (0..65000)
    MAX-ACCESS  read-write
    STATUS  current
    DESCRIPTION
    "Time of inactivity in seconds, after which the call will be
     disconnected. An x.25 link is considered inactive when there are
     no open calls on the link. A frame relay link is considered inactive
     when there is no traffic on the link.
     Possible values:
     0 - No disconnection on inactivity.
     1 - 65000 - Time period (in seconds).
     Default value: 180"
     ::= {pktSwchIsdnGlobal 3}

pktSwchIsdnGlobalStatus OBJECT-TYPE
    SYNTAX  INTEGER {
       valid(1),
