radFddiMACIndex OBJECT-TYPE  
      SYNTAX  Integer32 (1..65535)
      MAX-ACCESS  read-only 
      STATUS  current
      DESCRIPTION
      "A unique value for each MAC. Its value ranges 
      between 1 and the value of snmpFddiMACNumber."
      ::= { radFddiMACEntry 2 }


radFddiMACStation OBJECT-TYPE  
      SYNTAX  Integer32 (1..65535)
      MAX-ACCESS  read-only 
      STATUS  current
      DESCRIPTION
      "Indicates the Station inside the box to which the
      current MAC belongs. Its value ranges 
      between 1 and the value of radFddiStationNumber."
      ::= { radFddiMACEntry 3 }

radFddiMACTestResult OBJECT-TYPE  
      SYNTAX  Integer32 
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      " Indicates the result of a test performed on MAC.
      The value is a sum. It takes the value 0 if the result of the
      test is OK and if faults occur, 2 raised to a power is added 
      to the sum. The powers are according to the following table:
	     Fault       Power
      cardConfigError     0
      GeneralError        1
      MACError            2 "
      ::= { radFddiMACEntry 4 }

radFddiMACTokenCt OBJECT-TYPE
      SYNTAX Counter32
      MAX-ACCESS read-only
      STATUS current
      DESCRIPTION
      "Token Counts - Refer to ANSI MAC {73}."
      ::= { radFddiMACEntry 5 }
      
radFddiMACRingLoad OBJECT-TYPE 
      SYNTAX  Integer32             
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Current traffic value. The
       traffic is expressed in percents of the maximum FDDI traffic 
       in the range 1..20, expressed on a log(10) 
      scale ( value = 10*log10(percent) )."
      ::= { radFddiMACEntry 6 }

radFddiMACRingLoadBuffer OBJECT-TYPE
      SYNTAX  OCTET STRING
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Record of traffic for the last second with a resolution of 100 msec. The
       traffic is expressed in percents of the maximum FDDI traffic and each
      record contains 10 readings, in the range 1..20, expressed on a log(10) 
      scale ( value = 10*log10(percent) )."
      ::= { radFddiMACEntry 7 }

radFddiMACRingLoadTrsh OBJECT-TYPE 
      SYNTAX  Integer32               
      MAX-ACCESS  read-write
      STATUS  current
      DESCRIPTION
      "Traffic threshold value. When exceeded, a trap is send to the management
      console. Expressed in percents of the maximum FDDI traffic, in the 
      range 1..20, expressed on a log(10) scale."
      ::= { radFddiMACEntry 8 }

radFddiMACRingLoadPeak OBJECT-TYPE 
      SYNTAX  Integer32              
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Maximal traffic value since last reset of hub or relay. Expressed in per-
      cents of the maximum FDDI traffic, in the range 1..20, 
      expressed on log(10) scale."
      ::= { radFddiMACEntry 9 }

radFddiMACDownstreamNbr OBJECT-TYPE 
      SYNTAX  OCTET STRING  (SIZE (6))             
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "The MAC's downstream neighbor's long individual MAC address. The value
      shall be reported as '00 00 00 00 00 00' if it is unknown."
      ::= { radFddiMACEntry 10 }

radFddiMACRingStatus OBJECT-TYPE 
      SYNTAX  INTEGER {
	ringUp(1),
	ringDown(2)
      }      
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "Attribute which summs the status of the ring controlled by the current
      MAC."
      ::= { radFddiMACEntry 11 }

radFddiMACRingUtilization OBJECT-TYPE
     SYNTAX  Integer32
     MAX-ACCESS  read-only
     STATUS  current
     DESCRIPTION
     "Current traffic value. The traffic is expressed in percents of the maximum FDDI traffic." 
      ::= { radFddiMACEntry 12 }

radFddiMACRingPeakUtilization OBJECT-TYPE
     SYNTAX  Integer32
     MAX-ACCESS  read-only
     STATUS  current
     DESCRIPTION
     "Peak traffic value since las reset of the hub or relay. The traffic is expressed
     in percents of the maximum FDDI traffic." 
      ::= { radFddiMACEntry 13 }

