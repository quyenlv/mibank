   }

sysGenConfigIdx OBJECT-TYPE
      SYNTAX  Integer32 (1..255)
      MAX-ACCESS  read-only
      STATUS  current
      DESCRIPTION
      "This object indicates the Configuration number (1..255) 
       referred.
       Value of  255 specifies the TmpCnfg (Scratch).
       The write operation in this Table is valid only to the 
       TmpCnfg entry."
      ::= { sysGenKmuxEntry 1}

sysGenFlipIntegration OBJECT-TYPE
      SYNTAX INTEGER { notApplicable(1),
                       short        (2),
                       long         (3)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The parameter is relevant only for KM-2100 and only when 
       sysGenMlMode=redundancy, switchBackup, or priorityBumping."
      ::= { sysGenKmuxEntry 2}

sysGenMlMode OBJECT-TYPE
      SYNTAX INTEGER { single         (2),
                       redundancy     (3),
                       dualLink       (4),
                       priorityBumping(5),
                       switchBackup   (6)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The Main Link operation mode. Determines how many links are
       used (one/two) and how they are used."
      ::= { sysGenKmuxEntry 3}

sysGenFaType OBJECT-TYPE
      SYNTAX INTEGER { auto   (2),
                       manual (3)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "Frame Allocation type:
       auto - automatic frame allocation (by Kmux)
       manual - manually configured frame allocation (by user)."
      ::= { sysGenKmuxEntry 4}

sysGenLinkRecovery OBJECT-TYPE
      SYNTAX INTEGER { 
                       notApplicable (1),
                       auto          (2),
                       manual        (3)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The parameter determines the Link Recovery mode.
       The parameter is relevant only when the sysGenMlMode is either 
       switchBackup or priorityBumping."
      ::= { sysGenKmuxEntry 5}

sysGenLinkSourceClock OBJECT-TYPE
      SYNTAX INTEGER { none       (1),
                       kmxMlA  (103),
                       kmxMlB  (104)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The source clock for the links. The parameter is relevant 
       only for KM-2100 having a KDI or KDI/CH cards. ML-A or ML-B 
       are selectable only when sysGenMlMode=dual or priorityBumping."
      ::= { sysGenKmuxEntry 6}

sysGenMngBwA OBJECT-TYPE
      SYNTAX Integer32 
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The required management rate for ML-A, in bps. It is very important when a remote 
       unit is managed. 
       The selected value must be < ML-A Rate value.
       0 = None or not applicable
       800   bps = minimum value
       19200 bps = maximum value"
      ::= { sysGenKmuxEntry 7}

sysGenMngBwB OBJECT-TYPE
      SYNTAX Integer32 
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The required management rate for ML-B, in bps. It is very important when a remote 
       unit is managed. 
       The selected value must be < ML-B Rate value.
       0 = None or not applicable
       800   bps = minimum value
       19200 bps = maximum value"
      ::= { sysGenKmuxEntry 8}
 
sysGenAutoDialSlot OBJECT-TYPE
      SYNTAX Integer32 
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "This object indicates the IO-SLOTs that can be used to initiate Automatic 
      DialUp.
      This field will be applicable when KML.10 is a ML and its
      prtIsdnSwitchMode=auto.
      Each IO Slot is represented by a bit:   11 .............0
                                        MSB(IO12)       LSB(IO1)
      bit 0 (LSB) kmxIO1: '1'- used
                       '0'- not used
      bit 1       kmxIO2: '1'- used
                      '0'- not used
      bit 2       kmxIO3: '1'- used
      .       '0'- not used
      .
      bit 11(MSB) kmxIO12: '1'- used
                       '0'- not used
      
      The value will be: SUM of [2**n] ; n=bit number." 
      ::= { sysGenKmuxEntry 9}

sysGenResetBypassEnable OBJECT-TYPE
      SYNTAX INTEGER {
       notApplicable(1),
       disabled     (2),
       enabled      (3)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "Enable/Disable Reset Bypass operation when a KDI card exists
       and one of its links failed.
       notApplicable - when no KDI card exists in the Hub."
      ::= { sysGenKmuxEntry 10}

 
 
sysGenLinkRecoveryCmd OBJECT-TYPE
      SYNTAX INTEGER { 
                       noOp (1),
                       off  (2),
                       on   (3)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "Command to recover from ML failure.
       When 'on', the agent will perform manual link recovery and 
       reset the variable to 'off'. 
       The conditions for this action are: 
       sysGenMlMode= priorityBumping or switchBackup and also 
       sysGenLinkRecovery=manual(3)."
      ::= { generalKmux 2}

sysGenFlipMlCmd OBJECT-TYPE
      SYNTAX INTEGER { 
                       noOp (1),
                       off  (2),
                       on   (3)
      }
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "Command to flip the ML when sysGenMlMode=redundancy.
       When 'on', the agent will flip the ML and reset
       the variable to 'off'."
      ::= { generalKmux 3}
  
sysGenMonitorChannel OBJECT-TYPE
      SYNTAX Integer32
      MAX-ACCESS read-write
      STATUS current
      DESCRIPTION
      "The variable specifies the channel to be monitored and 
       presented on the front panel LEDs. The channel will be 
       specified according to: Slot*100+Ch. 
