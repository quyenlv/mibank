rcBridgeFdbVlanId OBJECT-TYPE
        SYNTAX    INTEGER (1..4094) 
        ACCESS    read-only
        STATUS    mandatory
        DESCRIPTION  ""
        ::= { rcBridgeFdbEntry 8 }

rcBridgeFdbTblSize OBJECT-TYPE  
        SYNTAX         	INTEGER  
        ACCESS         	read-only  
        STATUS        	mandatory  
        DESCRIPTION   	"Used to indicate the number of entries in 
                       	the bridge forwarding database table."    
