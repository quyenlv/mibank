        SYNTAX        EnableValue
        ACCESS        read-write
        STATUS        mandatory
        DESCRIPTION   "Used to enable/disable mlt functionality."
        ::= { rcMlt 3 }

rcMltIstSessionStatus OBJECT-TYPE
        SYNTAX        INTEGER {
                           up(1),
                           down(2)
                        }
        ACCESS        read-only
