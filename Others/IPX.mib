IPX DEFINITIONS ::= BEGIN

-- This MIB defines the management information for a system using the IPX
-- protocol.  The MIB consists of four groups:
--
--    1.  System Group - contains general information about all instances
--                       of IPX on the system
--
--    2.  Circuit Group - contains information about all circuits used by
--                        IPX on the system
--
--    3.  Forwarding Group - contains generic routing information that
--                           must be provided by any IPX routing protocol.
--
--    4.  Services Group - contains information about all known services.
--
-- The MIB is designed to support multiple instances of the IPX
-- protocol on one system via a system instance identifier which is the
-- primary index for every table in this MIB.
--
-- This MIB is designed to provide a basic framework for the management
-- of systems implementing the IPX protocol.  Additional MIBs may be
-- created (especially in the area of IPX routing protocols) to contain
-- more specific information.  Whenever possible, these additional MIBs
-- should follow the format of this IPX MIB.  Information in these MIBs
-- should be linked to this MIB via the use of the system instance 
-- identifier mentioned above.


IMPORTS
        DisplayString
                FROM RFC1213-MIB
        Counter
                FROM RFC1155-SMI
        OBJECT-TYPE
                FROM RFC-1212
	micom-oscar 
	    	FROM MICOM-OSCAR-MIB
        PhysAddress
                FROM RFC1213-MIB;

   mcmIpx     		OBJECT IDENTIFIER ::= { micom-oscar 4 }


-- Groups

ipxSystem OBJECT IDENTIFIER 		::= {mcmIpx 1}
ipxCircuit OBJECT IDENTIFIER 		::= {mcmIpx 2}
ipxForwarding OBJECT IDENTIFIER 	::= {mcmIpx 3}
ipxServices OBJECT IDENTIFIER 	::= {mcmIpx 4}
ripsap OBJECT IDENTIFIER 			::= {mcmIpx 5}
ripsapSystem OBJECT IDENTIFIER 	::= {ripsap 1}
ripsapCircuit OBJECT IDENTIFIER 	::= {ripsap 2}
nvmIpx OBJECT IDENTIFIER 			::= {mcmIpx 6}
nvmSystem OBJECT IDENTIFIER 		::= {nvmIpx 1}
nvmCircuit OBJECT IDENTIFIER 		::= {nvmIpx 2}
nvmForwarding OBJECT IDENTIFIER 	::= {nvmIpx 3}
ipxCntr OBJECT IDENTIFIER 			::= {mcmIpx 7}

-- Types

NetNumber       ::= OCTET STRING (SIZE(4))
OctetStringSiz2 ::= OCTET STRING (SIZE(2))

-- System Group
--   This group contains global information about each instance of IPX
--   running on one system.

-- Basic System Table
--   This table contains one entry for each instance of IPX running on
--   the system.  It contains the management information that should
--   be made available by all implementations of the IPX protocol.

ipxBasicSysTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF IPXBasicSysEntry
   	ACCESS      not-accessible
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The IPX System Table - basic Information.;;
	   HELP = ;
	   CAPABILITIES = NET_CFG, VPN_CFG;"
   ::= {ipxSystem 1}

ipxBasicSysEntry OBJECT-TYPE
	SYNTAX      IPXBasicSysEntry
   	ACCESS      not-accessible
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Each entry corresponds to one instance \
                  of IPX running on the system.;;
	   HELP = ;
	   CAPABILITIES = ;"
   	INDEX       {ipxBasicSysInstance}
   ::= {ipxBasicSysTable 1}

IPXBasicSysEntry ::= SEQUENCE {
                        ipxBasicSysInstance
                           INTEGER,
                        ipxBasicSysExistState
                           INTEGER,
                        ipxBasicSysNetNumber
                           NetNumber,
                        ipxBasicSysNode
                           OCTET STRING,
                        ipxBasicSysName
			   DisplayString,
                        ipxBasicSysInReceives
                           Counter,
                        ipxBasicSysInHdrErrors
                           Counter,
                        ipxBasicSysInUnknownSockets
                           Counter,
                        ipxBasicSysInDiscards
                           Counter,
                        ipxBasicSysInBadChecksums
                           Counter,
                        ipxBasicSysInDelivers
                           Counter,
                        ipxBasicSysNoRoutes
                           Counter,
                        ipxBasicSysOutRequests
                           Counter,
                        ipxBasicSysOutMalformedRequests
                           Counter,
                        ipxBasicSysOutDiscards
                           Counter,
                        ipxBasicSysOutPackets
                           Counter,
                        ipxBasicSysConfigSockets
                           Counter,
                        ipxBasicSysOpenSocketFails
                           Counter,
                        ipxBasicSysRouter
                           INTEGER,
                        ipxBasicSysSpoofing
                           INTEGER,
                        ipxBasicSysSpoofDataLinkType
                           INTEGER,
                        ipxBasicSysWanOptimize
                           INTEGER
                    }
                           
ipxBasicSysInstance OBJECT-TYPE
	SYNTAX      INTEGER (1)
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The unique identifier of the instance \
		  of IPX to which this row corresponds.; 
           HELP = This value may be written only when \
		  creating a new entry in the table.;
	   CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 1}

ipxBasicSysExistState OBJECT-TYPE
	SYNTAX      INTEGER {
                        off(1),
                        on(2)
                       }
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The validity of this entry in the IPX \
		  system table.; 
           HELP = Setting this field to off indicates that this \
                  entry may be deleted from the system table at \
		  the IPX implementation's discretion.;
	   CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 2}

ipxBasicSysNetNumber OBJECT-TYPE
	SYNTAX      NetNumber
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The network number portion of the IPX \
		  address of this system.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 3}

ipxBasicSysNode OBJECT-TYPE
	SYNTAX      OCTET STRING (SIZE(6))
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The node number portion of the IPX address \
		  of this system.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 4}

ipxBasicSysName OBJECT-TYPE
	SYNTAX      DisplayString(SIZE(0..48))
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The readable name for this system.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 5}

ipxBasicSysInReceives OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The total number of IPX packets received, \
		  including those received in error.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 6}

ipxBasicSysInHdrErrors OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX packets discarded \
		  due to errors in their headers, including \
		  any IPX packet with a size less than \
		  the minimum of 30 bytes.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 7}

ipxBasicSysInUnknownSockets OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX packets discarded \
		  because the destination socket was \
		  not open.;
	   HELP = ; 
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 8}

ipxBasicSysInDiscards OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX packets received but \
		  discarded .;
           HELP = This is due to reasons other than those \	
	          accounted for by ipxBasicSysInHdrErrors, \
		  and ipxBasicSysInUnknownSockets.;
	   CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 9}

ipxBasicSysInBadChecksums OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX packets received with \
		  incorrect checksums.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 10}

ipxBasicSysInDelivers OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The total number of IPX packets delivered \
		  locally, including packets from local \
		  applications.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 11}

ipxBasicSysNoRoutes OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of times no route to a destination \
		  was found.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 12}

ipxBasicSysOutRequests OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX packets supplied locally \
		  for transmission, not including any \
		  packets counted in ipxAdvForwPackets.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 13}

ipxBasicSysOutMalformedRequests OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX packets supplied locally \
		  that contained errors in their structure.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 14}

ipxBasicSysOutDiscards OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of outgoing IPX packets discarded \
		  due to reasons other than those accounted \
		  for in ipxBasicSysOutMalformedRequests, \
		  ipxAdvSysOutFiltered, and \
		  ipxAdvSysOutCompressDiscards.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 15}

ipxBasicSysOutPackets OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The total number of IPX packets transmitted.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 16}

ipxBasicSysConfigSockets OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The configured maximum number of IPX \
		  sockets that may be open at one time.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 17}

ipxBasicSysOpenSocketFails OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX socket open calls \
		  which failed.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 18}

ipxBasicSysRouter OBJECT-TYPE
	SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                       }
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Is the IPX being routed for this instance.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 19}

ipxBasicSysSpoofing OBJECT-TYPE
	SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                       }
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Is the IPX spoofing enabled for this \
		  instance.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 20}

ipxBasicSysSpoofDataLinkType OBJECT-TYPE
	SYNTAX      INTEGER {
                        raw(1),
                        type8023(2),
                        type2(3),
                        type8022(4)
                       }
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The DataLink type to Spoof on.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 21}

ipxBasicSysWanOptimize OBJECT-TYPE
	SYNTAX      INTEGER {
                        enabled(1),
                        disabled(2)
                       }
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Is the WAN optimization enabled for \
		  IPX for this instance.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxBasicSysEntry 22}


-- Advanced System Table
--   This table contains one entry for each instance of IPX running on
--   the system.  It contains the advanced management information that
--   may not be available from all implementations of the IPX protocol.

ipxAdvSysTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF IPXAdvSysEntry
   	ACCESS      not-accessible
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The IPX System table - advanced information.;
	   HELP = ;
           CAPABILITIES = NET_CFG, VPN_CFG;"
   ::= {ipxSystem 2}

ipxAdvSysEntry OBJECT-TYPE
	SYNTAX      IPXAdvSysEntry
   	ACCESS      not-accessible
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Each entry corresponds to one instance \
		  of IPX running on the system.;
	   HELP = ;
           CAPABILITIES = ;"
   	INDEX       {ipxAdvSysInstance}
   ::= {ipxAdvSysTable 1}

IPXAdvSysEntry ::= SEQUENCE {
                     ipxAdvSysInstance
                        INTEGER,
                     ipxAdvSysMaxPathSplits
                        INTEGER,
                     ipxAdvSysMaxHops
                        INTEGER,
                     ipxAdvSysInTooManyHops
                        Counter,
                     ipxAdvSysInFiltered
                        Counter,
                     ipxAdvSysNETBIOSPackets
                        Counter,
                     ipxAdvSysForwPackets
                        Counter,
                     ipxAdvSysOutFiltered
                        Counter,
                     ipxAdvSysCircCount
                        Counter,
                     ipxAdvSysDestCount
                        Counter,
                     ipxAdvSysServCount
                        Counter
                    }
                           
ipxAdvSysInstance OBJECT-TYPE
	SYNTAX      INTEGER (1)
   	ACCESS      read-write
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The unique identifier of the instance \
		  of IPX to which this row corresponds.; 
           HELP = This value may be written only when \
		  creating a new entry in the table.;
	   CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 1}

ipxAdvSysMaxPathSplits OBJECT-TYPE
	SYNTAX      INTEGER (1..32)
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The maximum number of paths with equal \
		  routing metric value which this instance \
		  of the IPX may split between when forwarding \
		  packets.;
	   HELP = ;
           CAPABILITIES = ;"
   	DEFVAL      { 1 }
   ::= {ipxAdvSysEntry 2}

ipxAdvSysMaxHops OBJECT-TYPE
	SYNTAX      INTEGER
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The maximum number of hops a packet \
		  may take.;
	   HELP = ;
           CAPABILITIES = ;"
   	DEFVAL      { 16 }
   ::= {ipxAdvSysEntry 3}

ipxAdvSysInTooManyHops OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX packets discarded \
		  due to exceeding the maximum hop count.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 4}

ipxAdvSysInFiltered OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of incoming IPX packets discarded \
		  due to filtering.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 5}

ipxAdvSysNETBIOSPackets OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of NETBIOS packets received.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 6}

ipxAdvSysForwPackets OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of IPX packets forwarded.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 7}

ipxAdvSysOutFiltered OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of outgoing IPX packets discarded \
		  due to filtering.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 8}

ipxAdvSysCircCount OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of circuits known to this \
		  instance of IPX.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 9}

ipxAdvSysDestCount OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of currently reachable destinations \
		  known to this instance of IPX.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 10}

ipxAdvSysServCount OBJECT-TYPE
	SYNTAX      Counter
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The number of services known to this \
		  instance of IPX.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxAdvSysEntry 11}


-- Circuit Group
--   This group contains management information for each circuit known
--   to this system.

-- Circuit Table
--   The Circuit table contains an entry for each circuit known to the
--   system.

ipxCircTable OBJECT-TYPE
	SYNTAX      SEQUENCE OF IPXCircEntry
   	ACCESS      not-accessible
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The Circuit table.;
	   HELP = ;
           CAPABILITIES = NET_CFG, VPN_CFG;"
   ::= {ipxCircuit 1}

ipxCircEntry OBJECT-TYPE
	SYNTAX      IPXCircEntry
   	ACCESS      not-accessible
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = Each entry corresponds to one circuit \
		  known to the system.;
	   HELP = ;
           CAPABILITIES = ;"
   	INDEX       {
                ipxCircSysInstance,
                ipxCircIndex
               }
   ::= {ipxCircTable 1}

IPXCircEntry ::= SEQUENCE {
                     ipxCircSysInstance
                        INTEGER,
                     ipxCircIndex
                        INTEGER,
                     ipxCircExistState
                        INTEGER,
                     ipxCircOperState
                        INTEGER,
                     ipxCircIfIndex
                        INTEGER,
                     ipxCircName
                        OCTET STRING,
                     ipxCircType
                        INTEGER,
                     ipxCircDataLinkType
                        INTEGER,
                     ipxCircDialName
                        OCTET STRING,
                     ipxCircLocalMaxPacketSize
                        INTEGER,
                     ipxCircCompressState
                        INTEGER,
                     ipxCircCompressSlots
                        INTEGER,
                     ipxCircStaticStatus
                        INTEGER,
                     ipxCircCompressedSent
                        Counter,
                     ipxCircCompressedInitSent
                        Counter,
                     ipxCircCompressedRejectsSent
                        Counter,
                     ipxCircUncompressedSent
                        Counter,
                     ipxCircCompressedReceived
                        Counter,
                     ipxCircCompressedInitReceived
                        Counter,
                     ipxCircCompressedRejectsReceived
                        Counter,
                     ipxCircUncompressedReceived
                        Counter,
                     ipxCircMediaType
                        OctetStringSiz2,
                     ipxCircNetNumber
                        NetNumber,
                     ipxCircStateChanges
                        Counter,
                     ipxCircInitFails
                        Counter,
                     ipxCircDelay
                        INTEGER,
                     ipxCircThroughput
                        INTEGER,
                     ipxCircNeighRouterName
                        OCTET STRING,
                     ipxCircNeighInternalNetNum
                        NetNumber
                    }

ipxCircSysInstance OBJECT-TYPE
	SYNTAX      INTEGER (1)
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The unique identifier of the instance \
		  of IPX to which this entry corresponds. \
           HELP = This value may be  written only when \
		  creating a new entry in the table.;
	   CAPABILITIES = ;"
   ::= {ipxCircEntry 1}

ipxCircIndex OBJECT-TYPE
	SYNTAX      INTEGER (1..16)
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The identifier of this circuit, unique \
		  within the instance of IPX.  This value \
		  may be written only when creating a \
		  new entry in the table.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxCircEntry 2}

ipxCircExistState OBJECT-TYPE
	SYNTAX      INTEGER {
                        off(1),
                        on(2)
                       }
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The validity of this circuit entry. \
		  A circuit with this value set to off \
		  is deleted from the table.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxCircEntry 3}

ipxCircOperState OBJECT-TYPE
	SYNTAX      INTEGER {
                        down(1),
                        up(2),
                        sleeping(3)
                       }
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The operational state of the circuit.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxCircEntry 4}

ipxCircIfIndex OBJECT-TYPE
	SYNTAX      INTEGER (1..16)
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The value of ifIndex for the interface \
		  used by this circuit.  This value may \
		  be written only when creating a new \
		  entry in the table.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxCircEntry 5}

ipxCircName OBJECT-TYPE
	SYNTAX      OCTET STRING (SIZE(0..48))
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The readable name for the circuit.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxCircEntry 6}

ipxCircType OBJECT-TYPE
	SYNTAX      INTEGER {
                        other(1),
                        broadcast(2),
                        ptToPt(3),
                        wanRIP(4),
                        unnumberedRIP(5),
                        dynamic(6),
                        wanWS(7)
                       }
   	ACCESS      read-write
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The type of the circuit.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxCircEntry 7}

ipxCircDataLinkType OBJECT-TYPE
	SYNTAX      INTEGER {
                        raw(1),
                        snap(2),
                        type2(3),
                        type8022(4)
                       }
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The type of Data Link on this circuit.;
	   HELP = ;
           CAPABILITIES = ;"
   ::= {ipxCircEntry 8}

ipxCircDialName OBJECT-TYPE
	SYNTAX      OCTET STRING (SIZE(0..48))
   	ACCESS      read-only
   	STATUS      mandatory
	DESCRIPTION
	  "NAME = ;
	   DESC = The symbolic name used to refernce the \
		  dialing information used to create \
		  this circuit.  This value may be written \
