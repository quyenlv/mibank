        	SYNTAX          IpAddress
                MAX-ACCESS      read-write
                STATUS          current
                DESCRIPTION
                        "Default gateway."
       
       ::= {whispSmConfig 5}               
       
        networkAccess		OBJECT-TYPE
        	SYNTAX          INTEGER{localIP(0),
        				pulicIP(1)}
                MAX-ACCESS      read-write
                STATUS          current
                DESCRIPTION
                        "Network accessibility. Public or local IP. 
                        For multipoint only."
       
       ::= {whispSmConfig 6}               
       
       
